-- This is a test control file

test_name 	vtbt-sneak
test_description Introduce VTBT violation by changing value of a constant integer attribute

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
compile_melted
compile_result 	ok

copy_raw 	test_rev1.e $CLUSTER test.e
compile_melted
compile_result	validity_error TEST VTBT

test_end
