system tst

root

    main (root_cluster): "make"

default

    assertion (require);
    -- On windows, with ISE Compiler v4.3 : uncomment the next line
    -- console_application (yes);
    -- Comment the next line, if you do not want to use
    -- a precompiled Eiffel Base library.
--    precompiled ("$ISE_EIFFEL/precomp/spec/$ISE_PLATFORM/base")

cluster

        root_cluster:           "$ISE_EIFFEL/examples/cecil/cecil-test"
                -- If you intend to modify any part of the example,
                -- copy all the files of $ISE_EIFFEL/examples/cecil/cecil-test
                -- into one of your directories, and replace
                -- $ISE_EIFFEL/examples/cecil/cecil-test by the name of
                -- that directory on the above line.
            visible
                MAIN;
            end

        all base :          "$ISE_EIFFEL/library/base"
			exclude
			   	"desc";"table_eiffel3";
			visible
                STRING;
                LINKED_LIST;
			end

end

