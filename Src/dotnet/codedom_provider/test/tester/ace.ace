system
	"ecd_test"

root
	TESTER_ROOT_CLASS: make_and_launch

default
	il_verifiable (no)
	cls_compliant (yes)
	dotnet_naming_convention (yes)
	use_cluster_name_as_namespace (no)
	use_all_cluster_name_as_namespace (yes)
	msil_generation_type ("exe")
	version ("1.0.0.0")
	msil_clr_version ("v1.1.4322")
	msil_use_optimized_precompile (yes)
	precompiled ("$EIFFEL_SRC\dotnet\codedom_provider\build_studio\EiffelSoftware.EiffelVision2")
	assertion (check)
	assertion (require)
	assertion (ensure)
	assertion (loop)
	assertion (invariant)
	disabled_debug (yes)
	disabled_debug ("dlg_dispatcher")
	disabled_debug ("vision2_windows")
	disabled_debug ("vision2_windows_gdi")
	disabled_debug ("wel")
	disabled_debug ("wel_gdi_count")
	disabled_debug ("wel_gdi_references")
	disabled_debug ("win_dispatcher")
	debug (no)
	line_generation (yes)
	profile (no)
	trace (no)
	check_vape (yes)
	console_application (no)
	syntax_warning (no)
	address_expression (no)
	array_optimization (no)
	dead_code_removal (yes)
	dynamic_runtime (no)
	exception_trace (no)
	inlining (no)
	multithreaded (no)


cluster
	all root_cluster:		"$EIFFEL_SRC\dotnet\codedom_provider\test\tester\gui"

	codedom_provider:		"$EIFFEL_SRC\dotnet\codedom_provider\common"

	library base_net:		"$EIFFEL_SRC\library.net\base"
		exclude
			"CVS";
		default
			namespace ("EiffelSoftware.Library.BaseNet")
			assertion (no)
			assertion (check)
		end

	library base:		"$EIFFEL_SRC\library\base"
		exclude
			"table_eiffel3"; "desc"; "classic"; "CVS";
		default
			namespace ("EiffelSoftware.Library.Base")
			assertion (no)
			assertion (check)
		end

	all wel:		"$EIFFEL_SRC\library\wel"
		exclude
			"spec"; "clib"; "classic"; "CVS";
		default
			namespace ("EiffelSoftware.Library.WEL")
			assertion (no)
			assertion (check)
		end

	all wel_net:		"$EIFFEL_SRC\library.net\wel"
		exclude
			"CVS";
		default
			namespace ("EiffelSoftware.Library.WELNet")
			assertion (no)
			assertion (check)
		end

	all vision2:		"$EIFFEL_SRC\library\vision2"
		exclude
			"Clib"; "gtk"; "gtk2"; "assign_attempt.e"; "ev_pixmap_imp_delegate.e"; "ev_pixmap_imp_loader.e"; "CVS";
		default
			namespace ("EiffelSoftware.Library.Vision2")
			assertion (no)
			assertion (check)
		end

	all vision2_net:		"$EIFFEL_SRC\library.net\vision2"
		exclude
			"CVS";
		default
			namespace ("EiffelSoftware.Library.Vision2Net")
			assertion (no)
			assertion (check)
		end

	all vision2_extensions:		"$EIFFEL_SRC\library\vision2_extension"
		exclude
			"CVS";
		default
			namespace ("EiffelSoftware.Library.Vision2Extension")
			assertion (no)
			assertion (check)
		end

external

	include_path:
		"$(EIFFEL_SRC)\library\wel\spec\windows\include",
		"$(EIFFEL_SRC)\library\vision2\spec\include"

	object:
		"$(EIFFEL_SRC)\library\wel\spec\$(ISE_C_COMPILER)\lib\ilwel.lib",
		"$(EIFFEL_SRC)\library\vision2\spec\$(ISE_C_COMPILER)\lib\ilvision2.lib",
		"$(EIFFEL_SRC)\library\vision2\spec\$(ISE_C_COMPILER)\lib\zlib.lib",
		"$(EIFFEL_SRC)\library\vision2\spec\$(ISE_C_COMPILER)\lib\libpng.lib"

end

