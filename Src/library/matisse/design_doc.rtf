\par \tab \tab a_stream: MT_STREAM
\par \tab \tab an_author: AUTHOR
\par \tab \tab a_book: BOOK
\par \tab \tab an_ep: MT_ENTRYPOINT
\par \tab do
\par \tab \tab !!an_ep.make_from_name("title", "BOOK")
\par \tab \tab a_book ?= an_ep.retrieve_first(book_title)
\par \tab \tab if a_book /= Void then
\par \tab \tab \tab a_stream := a_book.written_by.open_stream
\par \tab \tab \tab from
\par \tab \tab \tab \tab a_stream.start
\par \tab \tab \tab until
\par \tab \tab \tab \tab a_stream.exhausted
\par \tab \tab \tab loop
\par \tab \tab \tab \tab print(a_stream.item)
\par \tab \tab \tab \tab a_stream.forth
\par \tab \tab \tab end
\par \tab \tab \tab a_stream.close
\par \tab \tab end
\par \tab end
