-- This is a test control file

test_name 	 whip-that-ace
test_description After initial compilation, some clusters are removed from the Ace and then added back to it

copy_sub 	 Ace $TEST Ace
copy_raw 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

copy_sub 	 Ace_rev1 $TEST Ace
compile_melted
if not DOTNET compile_result 	 validity_error ABSTRACT_SPECIAL VTCT ; ANY VTCT VTCT ; ARRAY VTCT VTCT VTCT VTCT ; COM_FAILURE VTCT VTCT ; EXCEPTION VTCT ; FILE VTCT VTCT VTCT VTCT VTCT VTCT VTCT ; IMMUTABLE_STRING_8 VTCT VTCT ; INTEGER_INTERVAL VTCT VTCT VTCT VTCT ; IO_MEDIUM VTCT ; ISE_EXCEPTION_MANAGER VTCT VTCT VTCT ; NUMERIC VTCT ; READABLE_STRING_8 VTCT VTCT; READABLE_STRING_32 VTCT VTCT ; READABLE_STRING_GENERAL VTCT ; ROUTINE VTCT ; STRING_32 VTCT VTCT VTCT VTCT VTCT ; STRING_8 VTCT VTCT VTCT VTCT VTCT ; TUPLE VTCT VTCT VTCT ; TYPE VTCT
if DOTNET compile_result 	 validity_error ABSTRACT_SPECIAL VTCT ; ANY VTCT VTCT ; ARRAY VTCT VTCT VTCT VTCT ; COM_FAILURE VTCT VTCT ; DIRECTORY VTCT VTCT VTCT ; EXCEPTION VTCT ; FILE VTCT VTCT VTCT VTCT VTCT ; IMMUTABLE_STRING_8 VTCT VTCT ; INTEGER_INTERVAL VTCT VTCT VTCT VTCT ; IO_MEDIUM VTCT ; ISE_EXCEPTION_MANAGER VTCT VTCT ; NUMERIC VTCT ; RAW_FILE VTCT ; READABLE_STRING_32 VTCT VTCT; READABLE_STRING_8 VTCT VTCT ; READABLE_STRING_GENERAL VTCT ; ROUTINE VTCT ; STRING_32 VTCT VTCT VTCT VTCT VTCT ; STRING_8 VTCT VTCT VTCT VTCT VTCT ; TUPLE VTCT ; TUPLE VTCT ; TYPE VTCT VTCT

copy_sub 	 Ace $TEST Ace
resume_compile
compile_result 	 ok

test_end
