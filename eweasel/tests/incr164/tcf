-- This is a test control file

test_name 	 delete-parent-vtcg-sneak
test_description After initial compilation, an inheritance clause is deleted, thereby introducing a VTCG error because actual generic #2 no longer conforms to actual generic #1, as required by generic class. The compiler does not detect the error.

define PARENT "DOUBLE undefine out, is_equal end"
copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
copy_raw 	test1.e $CLUSTER test1.e
copy_sub 	test2.e $CLUSTER test2.e
copy_raw 	test3.e $CLUSTER test3.e
compile_melted
compile_result 	validity_error TEST2 VIFI(1)

define PARENT ""
copy_sub 	test2.e $CLUSTER test2.e
resume_compile
compile_result 	validity_error TEST1 VTCG

test_end
