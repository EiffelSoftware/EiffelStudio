system
	"EiffelSoftware.CodeDom.Splitter"

root
	EIFFEL_PARSER: make

default
	multithreaded (no)
	assertion (no)
	msil_generation (yes)
	line_generation (no)
	il_verifiable (yes)
	cls_compliant (yes)
	dotnet_naming_convention (yes)
	use_cluster_name_as_namespace (yes)
	use_all_cluster_name_as_namespace (no)
	msil_generation_type ("dll")
	msil_key_file_name ("$EIFFEL_SRC\dotnet\helpers\isekey.snk")
	console_application (yes)
	precompiled ("$EIFFEL_SRC\dotnet\codedom_provider\build_studio\EiffelSoftware.CodeDom.Vision2")
	msil_use_optimized_precompile (yes)
  	version ("2.0.1.1704")
 	msil_clr_version ("v1.0.3705")

cluster
	all thread:		"$EIFFEL_SRC\library\thread"
		exclude
			"classic"; "CVS"; 
		default
			namespace ("EiffelSoftware.Library.Thread")
		end

	all thread_net:		"$EIFFEL_SRC\library.net\thread"
		exclude
			"CVS"; 
		default
			namespace ("EiffelSoftware.Library.ThreadNet")
		visible
			THREAD
			end
			MUTEX
			end
		end

	all eclop:		"$ECLOP\eclop"
		default
			namespace ("Eclop")
		visible
			COMMAND_LINE_SYNTAX
			end
			COMMAND_LINE_PARSER
			end
		end

	all gobo:		"$EIFFEL_SRC\library\gobo\library"
		exclude
			"hact"; "se"; "ve"; "base"; "CVS"; "other";
		default
			namespace ("Gobo")
		visible
			RX_PCRE_REGULAR_EXPRESSION
			end
		end

	all ast:		"$EIFFEL_SRC\Eiffel\common"
		exclude
			"CVS";
		default
			namespace ("EiffelSoftware.Compiler.Parser")
		end

end
