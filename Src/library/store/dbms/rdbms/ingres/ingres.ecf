<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="ingres" uuid="42078327-FFEB-4A2A-94C6-05166F2DDFCD" library_target="db">
  <target name="db">
    <description>
      INGRES access library for EiffelStore.
      Copyright (c) 1984-2006, Eiffel Software and others.
      Eiffel Forum License v2 (see http://www.eiffel.com/licensing/forum.txt).
    </description>
    <root all_classes="true"/>
    <file_rule>
      <exclude>/\.svn</exclude>
    </file_rule>
    <external_object location="$(ISE_LIBRARY)\library\store\spec\$(ISE_C_COMPILER)\$(ISE_PLATFORM)\lib\ingres_store.lib">
      <condition>
        <platform value="windows"/>
      </condition>
    </external_object>
    <external_object location="$(II_SYSTEM)\ingres\lib\ingres.lib">
      <condition>
        <platform value="windows"/>
      </condition>
    </external_object>
    <external_object location="-L$(ISE_LIBRARY)/library/store/spec/$(ISE_PLATFORM)/lib">
      <condition>
        <platform excluded_value="windows"/>
      </condition>
    </external_object>
    <external_object location="-lingres_store">
      <condition>
        <platform excluded_value="windows"/>
      </condition>
    </external_object>
    <external_object location="-L$(II_SYSTEM)/ingres/lib">
      <condition>
        <platform excluded_value="windows"/>
      </condition>
    </external_object>
    <external_object location="-lingres">
      <condition>
        <platform excluded_value="windows"/>
      </condition>
    </external_object>
    <library name="base" location="$ISE_LIBRARY\library\base\base.ecf"/>
    <library name="store" location="$ISE_LIBRARY\library\store\store.ecf"/>
    <library name="time" location="$ISE_LIBRARY\library\time\time.ecf"/>
    <cluster name="db" location=".\"/>
  </target>
</system>
