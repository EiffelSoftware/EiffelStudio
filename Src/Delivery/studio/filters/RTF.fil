--!-----------------------------------------------------------------------------
--! Rich Text filter for EiffelStudio documentation output.
--!-----------------------------------------------------------------------------
--! $Date$
--! $Revision$
--!-----------------------------------------------------------------------------

Suffix              |rtf

-- Notes about the color table.
-- The colors are as follows : undefined, black, white and then starting with color 3:
-- Color Index			Used for

--	3			Comments
--	4			Class
--	5			Feature names
--	6			Keywords

-- Major syntactical constructs

Cluster_declaration, Class_declaration	|%
%{\rtf1\ansi\ansicpg1252\deflang1033{\fonttbl{\f0\fscript\fcharset0 Courier;}}{\colortbl ;\red0\green0\blue0;\red255\green255\blue255;\red128\green0\blue0;\red0\green0\blue255;\red0\green128\blue0;\red0\green0\blue128;}%N%
%{\cf3\f0\fs16 {\tab}{\tab}{\tab}%-%- Automatic generation produced by ISE Eiffel %-%-%N%
%{\line}%N%
%{*}%N%
%{\line}%N%
%{\cf3 {\tab}{\tab}{\tab}%-%- Generated by ISE Eiffel %-%-}%N%
%{\line}%N%
%{\cf3{\tab}{\tab}{\tab}%-%- For more details: http://www.eiffel.com %-%-}%N%
%}}%N

-- General specifications

keyword                 |{\b\cf6 *}
symbol                  |{\cf1 *}
comment                 |{\cf3 *}
class_name		|{\cf4 *}
non_generated_class	|{\cf4 *}
non_generated_feature	|{\cf5 *}
features		|{\cf5 *}
keyword_features	|{\cf1 *}
symbol_features		|{\cf1 *}
local_symbol		|{\cf5 *}
feature_declaration	|{\cf5 *}
quoted			|{\cf5 *}
tab                     |{\tab}
new_line                |{\line}%N
assertion_tag		|{\cf1 *}
indexing_tag		|{\cf1 *}

-- Symbols
.                       |{\b\cf1 .}

-- Meta-characters

Escape{                 |\{
Escape}                 |\}
Escape\                 |\\

