-- WEL Ace file (precompilation for Windows 9x/Me/2000)

system
	wel

root
	ANY

cluster 
		-- EiffelBase
	all base:		"$ISE_EIFFEL\library\base"

		-- WEL
	all wel:		"$ISE_EIFFEL\library\wel"

external
	include_path:	"$(ISE_EIFFEL)\library\wel\spec\windows\include"
	object: 		"$(ISE_EIFFEL)\library\wel\spec\$(ISE_COMPILER)\lib\wel.lib"

end -- system wel
