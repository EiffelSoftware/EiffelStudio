-- This is a test control file

test_name 	frozen-floor-ceiling
test_description Floor and ceiling called on a REAL do not work correctly when code is frozen.

define INST ""
copy_sub 	Ace $TEST Ace
copy_sub 	test.e $CLUSTER test.e
compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work 	NONE exec_output1
execute_result 	ok
compare		exec_output1 output

define INST "r := 50.0"
copy_sub 	Ace $TEST Ace
copy_sub 	test.e $CLUSTER test.e
compile_melted
compile_result 	ok

execute_work 	NONE exec_output2
execute_result 	ok
compare		exec_output2 output

test_end
