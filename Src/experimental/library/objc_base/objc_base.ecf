		<root all_classes="true"/>
		<file_rule>
			<exclude>/\.svn$</exclude>
			<exclude>/EIFGEN.{0,1}$</exclude>
			<exclude>/temp$</exclude>
		</file_rule>
		<option warning="true">
		</option>
		<external_include location="$(ISE_LIBRARY)/library/objc_base/Clib -ObjC"/>
