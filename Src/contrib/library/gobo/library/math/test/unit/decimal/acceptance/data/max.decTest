version: 2.15

-- we assume that base comparison is tested in compare.decTest, so
-- these mainly cover special cases and rounding

extended:    1
precision:   9
rounding:    half_up
maxExponent: 999

-- sanity checks
maxx001 max  -2  -2  -> -2
maxx002 max  -2  -1  -> -1
