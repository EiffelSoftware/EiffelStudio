note
	description: "Objects that represent an automatically created NONE id"
	legal: "See notice at end of class."
	status: "See notice at end of class."
	author: ""
	date: "$Date$"
	revision: "$Revision$"

class
	NONE_ID_AS

inherit
	ID_AS
		redefine
			process,
			text
		end

create
	make

feature {NONE} -- Initialization

	make
			-- Create.
		do
			initialize_from_id ({PREDEFINED_NAMES}.none_class_name_id)
		end

feature -- Visitor

	process (v: AST_VISITOR)
			-- process current element.
		do
			v.process_none_id_as (Current)
		end

feature {INTERNAL_COMPILER_STRING_EXPORTER} -- Roundtrip

	text (a_list: LEAF_AS_LIST): STRING
			-- Literal text of this token, which is stored in `a_list'
		do
				-- In view of roundtrip parser, text of a NONE_ID_AS is empty because this
				-- node is not in source code.
			Result := ""
		end

