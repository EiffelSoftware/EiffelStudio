-- This is a test control file

test_name 	 change-agent-return-type
test_description An expanded class has a call to an agent, which consists of a call to an inherited function.  After initial compilation the return type of the inherited function is changed and changes are melted.  System execution then dies with a segmentation violation.

define RETURN_TYPE "STRING"
copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
copy_sub 	test1.e $CLUSTER test1.e
copy_raw 	test2.e $CLUSTER test2.e
compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work 	NONE 	exec_output1
execute_result 	ok
compare		exec_output1 output1

define RETURN_TYPE "BOOLEAN"
copy_sub 	test1.e $CLUSTER test1.e
compile_melted
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work 	NONE 	exec_output2
execute_result 	ok
compare		exec_output2 output2

test_end
