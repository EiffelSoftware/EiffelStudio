system
	header_ctrl_demo
	
root 
	HEADER_CTRL_DEMO (root_cluster): "make"

default
	assertion (all)

cluster 
	root_cluster:	"$EIFFEL4\examples\wel\header_ctrl"

		-- WEL
	all wel: 		"$EIFFEL4\library\wel"

		-- EiffelBase
	all base: 		"$EIFFEL4\library\base"
		exclude
			"table_eiffel3"
			"desc"
		end

external
	include_path:	"$EIFFEL4\library\wel\spec\windows\include"
	object: 		"$EIFFEL4\library\wel\spec\$(COMPILER)\lib\wel.lib"

end -- header_ctrl_demo
