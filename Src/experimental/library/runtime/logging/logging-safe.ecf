<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-13-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-13-0 http://www.eiffel.com/developers/xml/configuration-1-13-0.xsd" name="logging" uuid="42078327-FFEB-4A2A-94C6-05166F2DDFCE" library_target="logging">
	<target name="logging">
		<description>Logging Cluster - Provide 1 interface to various logging mechanisms
			Copyright (c) 2010-2013, ITPassion Ltd, Eiffel Software and others
			Eiffel Forum License v2</description>
		<root all_classes="true"/>
		<file_rule>
			<exclude>/\.svn$</exclude>
			<exclude>/\.git$</exclude>
			<exclude>/EIFGENs$</exclude>
		</file_rule>
		<option warning="true" void_safety="all" namespace="EiffelSoftware.Library">
		</option>
		<external_include location="$(ISE_LIBRARY)/library/runtime/logging/Clib"/>
		<library name="base" location="$ISE_LIBRARY\library\base\base-safe.ecf"/>
		<library name="time" location="$ISE_LIBRARY\library\time\time-safe.ecf"/>
		<cluster name="logging" location=".\" recursive="true">
			<option namespace="Logging">
			</option>
			<file_rule>
				<exclude>/spec$</exclude>
				<exclude>/doc$</exclude>
				<exclude>/Clib$</exclude>
			</file_rule>
		</cluster>
	</target>
</system>
