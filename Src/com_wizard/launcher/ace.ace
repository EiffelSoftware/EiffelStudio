system
	"com_wizard_launcher"

root
	ROOT_CLASS: make

default
	assertion (require)
	console_application (no)

cluster
	root_cluster:		"$EIFFEL_SRC\com_wizard\launcher"

	library base:		"$EIFFEL_SRC\library\base"
		exclude
			"table_eiffel3"; "desc"; 
		end

	library wel:		"$EIFFEL_SRC\library\wel"
		exclude
			"CVS"; "Clib"; 
		end

external
	include_path:
		"$(EIFFEL_SRC)\library\wel\spec\windows\include"

	object:
		"$(EIFFEL_SRC)\library\wel\spec\$(ISE_C_COMPILER)\lib\wel.lib"

end
