	<HEAD>EiffelBuild Resources</HEAD>
	<TOPIC TOPIC_ID="Global_preferences" ICON="icon_preferences_folder">
		<HEAD>Global preferences</HEAD>
		<TEXT DESCRIPTION="Show tip of the day at start up">show_tip_of_the_day<BOOLEAN>True</BOOLEAN></TEXT>
		<TEXT DESCRIPTION="Number of projects kept in the recent project list" IMMEDIATE_EFFECT="yes">number_of_recent_projects<INTEGER>10</INTEGER></TEXT>
	</TOPIC>
	
	<TOPIC TOPIC_ID="Graphics" ICON="icon_preferences_folder">
		<HEAD>Graphical Elements</HEAD>
		<TOPIC TOPIC_ID="Dialogs" ICON="icon_preferences_folder">
			<HEAD>Dialogs</HEAD>
			<TEXT DESCRIPTION="Display warning when adding repeated absolute pixmap">show_repeated_absolute_constant_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning create a new relative pixmap constant with a directory constant that does not already exist">show_create_new_directory_constant_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when a constant selection has been rejected due to an out of bounds value">show_invalid_constant_selection_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when converting a deleted constant reference to a manifest value">show_constant_manifest_conversion_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when project location changed">show_project_location_changed_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when deleting via the keyboard">show_deleting_keyboard_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when deleting a directory containing one or more windows">show_deleting_directories_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when deleting final directory containing root window">show_deleting_final_directory_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when adding a directory that already exists on the disk">show_adding_existing_directory_warning<BOOLEAN>True</BOOLEAN></TEXT>
			<TEXT DESCRIPTION="Display warning when changing client status of already generated window">show_changing_client_type_warning<BOOLEAN>True</BOOLEAN></TEXT>
		</TOPIC>
	</TOPIC>
