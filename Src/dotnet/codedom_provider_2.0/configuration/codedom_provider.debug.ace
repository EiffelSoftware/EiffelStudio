system
	"EiffelSoftware.CodeDom"

root
	CODE_DOM_PROVIDER: default_create

default
	multithreaded (no)
	precompiled ("$EIFFEL_SRC\dotnet\codedom_provider_2.0\build_studio_debug\EiffelSoftware.CodeDom.CacheBrowser")
	msil_use_optimized_precompile (yes)
	assertion (require)
	msil_generation (yes)
	line_generation (yes)
	il_verifiable (yes)
	cls_compliant (yes)
	dotnet_naming_convention (yes)
	use_cluster_name_as_namespace (no)
	use_all_cluster_name_as_namespace (no)
	msil_generation_type ("dll")
  	version ("5.7.0224.0")
	msil_key_file_name ("$EIFFEL_SRC\dotnet\helpers\isekey.snk")
	msil_assembly_compatibility ("None")
 	msil_clr_version ("v2.0.50727")

cluster
	all interface:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\interface"
		exclude
			".svn"; 
		default
			namespace ("EiffelSoftware.CodeDom")
		end

	all merger:			"$EIFFEL_SRC\tools\partial_classes_merger\src"
		exclude
			".svn";
		default
			namespace ("EiffelSoftware.Library.Merger")
		end

	all eclop:		"$EIFFEL_SRC\free_add_ons\eclop\eclop"
		exclude
			".svn"; 
		default
			namespace ("EiffelSoftware.Library.Merger")
		end

	all compiler:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\compiler"
		exclude
			".svn"; "interface"; 
		default
			namespace ("EiffelSoftware.CodeDom.Compiler")
		end

	compiler_interface:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\compiler\interface"
		exclude
			".svn";
		default
			namespace ("EiffelSoftware.CodeDom")
		end

	all code_generator:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\code_generator"
		exclude
			".svn"; "interface"; 
		default
			namespace ("EiffelSoftware.CodeDom.CodeGenerator")
		end

	code_generator_interface:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\code_generator\interface"
		exclude
			".svn";
		default
			namespace ("EiffelSoftware.CodeDom")
		end

	all parser:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\parser"
		exclude
			".svn"; "interface"; 
		default
			namespace ("EiffelSoftware.CodeDom.Parser")
		end

	parser_interface:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\parser\interface"
		exclude
			".svn";
		default
			namespace ("EiffelSoftware.CodeDom")
		end

	env_interp:		"$EIFFEL_SRC\eiffel\lace\env"
		exclude
			".svn";
		default
			namespace ("EiffelSoftware.CodeDom.Support")
		end

	shared_env:		"$EIFFEL_SRC\eiffel\eiffel\shared"
		exclude
			".svn"; "shared_names_heap.e"; "shared_error_handler.e";
		default
			namespace ("EiffelSoftware.CodeDom.Support")
		end

	support:		"$EIFFEL_SRC\library"
		exclude
			".svn";
		default
			namespace ("EiffelSoftware.CodeDom.Support")
		end

	library gobo:		"$GOBO\library"
		exclude
			"hact"; "other"; "se"; "ve"; "expat"; ".svn"; 
		default
			namespace ("EiffelSoftware.Library.Gobo")
		end

	all ast:		"$EIFFEL_SRC\Eiffel\common"
		exclude
			".svn";
		default
			namespace ("EiffelSoftware.CodeDom.Support")
		end

	eiffel_parser_errors (ast):		"$EIFFEL_SRC\Eiffel\API\error\misc"
		exclude
			"error_handler.e"; "syntax_error.e"; ".svn"
		default
			namespace ("EiffelSoftware.CodeDom.Support")
		end
		
end -- system EiffelSoftware.CodeDom

