-- This is a test control file

test_name 	invalid-generic-constraint
test_description A generic class with a single formal generic parameter which is constrained by a class with an invalid actual generic parameter (`STRING [STRING]')

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
copy_raw 	test1.e $CLUSTER test1.e
compile_melted
compile_result 	validity_error TEST1 VTUG(1)

test_end

