<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-11-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-11-0 http://www.eiffel.com/developers/xml/configuration-1-11-0.xsd" name="bzip2" uuid="1EA72614-87F8-49B8-B985-F828900161AA" library_target="bzip2">
	<target name="bzip2">
		<root all_classes="true"/>
		<option warning="true">
			<assertions precondition="true" postcondition="true" check="true" invariant="true" loop="true" supplier_precondition="true"/>
		</option>
		<setting name="console_application" value="true"/>
		<library name="base" location="$ISE_LIBRARY/library/base/base-safe.ecf"/>
		<external_include location="$(EIFFEL_LIBRARY)\bzip2\spec\include">
            <condition>
                <platform value="windows"/>
            </condition>
        </external_include>
		<external_object location="libbz2.lib">
	        <condition>
                <platform value="windows"/>
            </condition>
        </external_object>
        <external_object location="-lbz2"> 
            <condition> 
                <platform excluded_value="windows"/> 
            </condition>
        </external_object>
		<cluster name="bzip2" location="library\" recursive="true">
			<file_rule>
				<exclude>/EIFGENs$</exclude>
				<exclude>/.svn$</exclude>
				<exclude>/CVS$</exclude>
			</file_rule>
		</cluster>
	</target>
</system>
