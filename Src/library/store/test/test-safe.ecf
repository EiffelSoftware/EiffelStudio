<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-9-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-9-0 http://www.eiffel.com/developers/xml/configuration-1-9-0.xsd" name="test_store" uuid="345FC7FB-F7D9-4134-A002-2C66BEECEAEF">
	<target name="common" abstract="true">
		<file_rule>
			<exclude>/EIFGENs$</exclude>
			<exclude>/CVS$</exclude>
			<exclude>/.svn$</exclude>
		</file_rule>
		<setting name="executable_name" value="test_store"/>
		<setting name="concurrency" value="thread"/>
		<library name="base" location="$ISE_LIBRARY\library\base\base-safe.ecf" readonly="false"/>
		<library name="json" location="$ISE_EIFFEL\contrib\library\text\parser\json\library\json.ecf"/>
		<library name="store" location="$ISE_LIBRARY\library\store\store-safe.ecf" use_application_options="true"/>
		<library name="testing" location="$ISE_LIBRARY\library\testing\testing-safe.ecf" readonly="false"/>
		<library name="time" location="$ISE_LIBRARY\library\time\time-safe.ecf"/>
		<cluster name="support" location=".\src\support\"/>
	</target>
	<target name="test_mysql" extends="common">
		<root class="TEST_STORE" feature="make"/>
		<tests name="autotests" location=".\src\autotests\"/>
		<library name="mysql" location="$ISE_LIBRARY\library\store\dbms\rdbms\mysql\mysql-safe.ecf" use_application_options="true"/>
		<cluster name="src" location=".\src\"/>
		<cluster name="mysql_spec" location=".\src\spec\mysql\"/>
	</target>
	<target name="test_odbc" extends="common">
		<root class="TEST_STORE" feature="make"/>
		<tests name="autotests" location=".\src\autotests\"/>
		<library name="odbc" location="$ISE_LIBRARY\library\store\dbms\rdbms\odbc\odbc-safe.ecf" use_application_options="true"/>
		<cluster name="src" location=".\src\"/>
		<cluster name="odbc_spec" location=".\src\spec\odbc\"/>
	</target>
</system>
