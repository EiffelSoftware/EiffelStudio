system
	test

root
	EV_TEST: make_and_launch

default
	assertion (all)

cluster
	ws:                   "$VISION_PRERELEASE"

	all wel (ws):         "$\wel"

	all vision2 (ws):     "$\vision2"
		exclude
			"gtk"
			"obsolete"
		end

	all base (ws):        "$\base"
		exclude
			"table_eiffel3"
		end

external
	include_path:
		"$VISION_PRERELEASE\wel\spec\windows\include",
		"$VISION_PRERELEASE\vision2\spec\include"
	object:
		"$VISION_PRERELEASE\wel\spec\$(COMPILER)\lib\wel.lib",
		"$VISION_PRERELEASE\libpng\libpng.lib",
		"$VISION_PRERELEASE\zlib\zlib.lib",
		"$VISION_PRERELEASE\vision2\spec\$(COMPILER)\lib\vision2.lib"
end
