system
	eiffel_time_calendar

root
	calculator: "make"

default
	assertion (all)
	precompiled ("$ISE_LIBRARY\precomp\spec\$ISE_PLATFORM\base")

cluster
	root_cluster:		"$ISE_LIBRARY\examples\time\calculator"


	eiffel_time:		"$ISE_LIBRARY\library\time"
	localizer:		"$ISE_LIBRARY\library\time\format"
	language:		"$ISE_LIBRARY\library\time\format\english"

	all base:		"$ISE_LIBRARY\library\base"
		exclude
			"desc"; "table_eiffel3"; 
		end

end
