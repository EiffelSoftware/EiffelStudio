system
	test_all_widgets

root
	DEMO (root_cluster): "make"

default
	assertion (all)

cluster
	root_cluster:	"$ISE_EIFFEL/examples/vision/test_all_widgets"

	-- EiffelBase
	all base: 		"$ISE_EIFFEL\library\base"
		exclude
			"table_eiffel3";
			"desc"
		end

		-- WEL
	all wel:		"$ISE_EIFFEL\library\wel"

		-- EiffelVision
	all vision:		"$ISE_EIFFEL\library\vision"
		exclude
			"obsolete"; "motif"; "gtk"; "x"; "CVS"
		default
			assertion(no);
		end	

external
	include_path: "$(ISE_EIFFEL)\library\wel\spec\windows\include";

	object: "$(ISE_EIFFEL)\library\wel\spec\$(ISE_C_COMPILER)\lib\wel.lib";

end
