-- This is a test control file

test_name 	 whip-that-ace
test_description After initial compilation, some clusters are removed from the Ace and then added back to it

copy_sub 	 Ace $TEST Ace
copy_raw 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

copy_sub 	 Ace_rev1 $TEST Ace
compile_melted
if not DOTNET compile_result 	 validity_error ABSTRACT_SPECIAL VTCT ; ANY VTCT VTCT ; ARRAY VTCT VTCT VTCT VTCT VTCT ; COM_FAILURE VTCT VTCT ; CONSOLE VTCT ; CONSOLE VTCT ; EXCEPTION VTCT VTCT ; FILE VTCT VTCT VTCT VTCT VTCT VTCT ; FILE_INFO VTCT VTCT ; FILE_ITERATION_CURSOR VTCT ; IMMUTABLE_STRING_32 VTCT VTCT VTCT ; IMMUTABLE_STRING_8 VTCT VTCT ; INTEGER_INTERVAL VTCT VTCT VTCT VTCT ; IO_MEDIUM VTCT ; ISE_EXCEPTION_MANAGER VTCT VTCT VTCT ; NATIVE_STRING VTCT VTCT VTCT ; NUMERIC VTCT ; PATH VTCT VTCT VTCT VTCT VTCT VTCT ; PLAIN_TEXT_FILE VTCT VTCT VTCT ; RAW_FILE VTCT ; READABLE_STRING_8 VTCT VTCT ; READABLE_STRING_32 VTCT VTCT ; READABLE_STRING_GENERAL VTCT VTCT VTCT VTCT ; ROUTINE VTCT VTCT ; SPECIAL VTCT VTCT ; STRING_32 VTCT VTCT VTCT VTCT VTCT VTCT; STRING_8 VTCT VTCT VTCT VTCT VTCT VTCT ; STRING_SEARCHER VTCT ; TUPLE VTCT VTCT VTCT VTCT ; TYPE VTCT ; UTF_CONVERTER VTCT VTCT
if DOTNET compile_result		validity_error ABSTRACT_SPECIAL VTCT ; ANY VTCT VTCT ; ARGUMENTS_32 VTCT ; ARRAY VTCT VTCT VTCT VTCT VTCT ; COM_FAILURE VTCT VTCT ; CONSOLE VTCT ; CONSOLE VTCT ; FILE VTCT VTCT VTCT VTCT ; IMMUTABLE_STRING_32 VTCT VTCT VTCT; IMMUTABLE_STRING_8 VTCT VTCT ; INTEGER_INTERVAL VTCT VTCT VTCT VTCT ; IO_MEDIUM VTCT ; ISE_EXCEPTION_MANAGER VTCT ; NATIVE_STRING VTCT VTCT VTCT ; NUMERIC VTCT ; PATH VTCT VTCT VTCT VTCT VTCT VTCT ; PLAIN_TEXT_FILE VTCT VTCT VTCT ; READABLE_STRING_32 VTCT VTCT ; READABLE_STRING_8 VTCT VTCT ; READABLE_STRING_GENERAL VTCT VTCT VTCT VTCT ; SPECIAL VTCT VTCT ; STRING_32 VTCT VTCT VTCT VTCT VTCT VTCT ; STRING_8 VTCT VTCT VTCT VTCT VTCT VTCT ; STRING_SEARCHER VTCT ; TUPLE VTCT VTCT VTCT; TYPE VTCT VTCT ; UTF_CONVERTER VTCT VTCT

copy_sub 	 Ace $TEST Ace
