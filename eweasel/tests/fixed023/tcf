-- This is a test control file

test_name 	 invalid-actual-generic3
test_description An actual generic parameter which is invalid (because it is itself a generic class but no actual generic parameters are supplied)

copy_sub 	 Ace $TEST Ace
define		 ACTUAL TEST1
copy_sub 	 test.e  $CLUSTER test.e
copy_raw 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 validity_error TEST VTUG(2)

define		 ACTUAL "TEST1 [TEST]"
copy_sub 	 test.e  $CLUSTER test.e
resume_compile
compile_result 	 ok

test_end
