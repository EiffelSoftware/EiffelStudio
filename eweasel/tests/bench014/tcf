test_name	 benchmark-scoop-synthetic-winnow
test_description A synthetic SCOOP benchmark: winnow.

copy_sub	Ace                 $TEST    Ace
copy_raw	main.e              $CLUSTER main.e
copy_raw	parfor_worker.e     $CLUSTER parfor_worker.e
copy_raw	tuple_sorter.e      $CLUSTER tuple_sorter.e
copy_raw	value3.e            $CLUSTER value3.e
copy_raw	value3_comparator.e $CLUSTER value3_comparator.e
copy_raw	test.in	            $TEST    test.in


compile_final
compile_result 	ok
c_compile_final
c_compile_result ok

define_directory EIFGEN  $TEST   EIFGENs
define_directory TARGET  $EIFGEN test
define_directory FCODE   $TARGET F_code
copy_bin	 test.in $FCODE  test.in

execute_final	NONE exec_output_f -i test.in -bench
execute_result	ok

test_end
