system
	sample

root
	ROOT_CLASS: "make"

default
	assertion (all)
	address_expression (yes)

cluster

	root_cluster: "$ISE_EIFFEL/examples/Eiffel2Java";

		-- EiffelBase
	all base:						"$ISE_EIFFEL/library/base"
		exclude
			"desc";"table_eiffel3"
		end

	java:		"$ISE_EIFFEL/library/Eiffel2Java"

external
	object:
		"$(ISE_EIFFEL)/library/Eiffel2Java/spec/$ISE_PLATFORM/lib/e2j.obj",
		"D:/apps/jdk1.1.8/lib/javai.lib"
		

end
