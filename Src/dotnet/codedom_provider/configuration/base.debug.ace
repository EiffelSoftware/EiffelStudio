system
	"EiffelSoftware.CodeDom.Base"

root
	ANY

default
 	assertion (require)
 	msil_generation (yes)
 	il_verifiable (yes)
 	cls_compliant (yes)
 	line_generation (yes)
 	dotnet_naming_convention (yes)
 	msil_generation_type ("dll")
	msil_key_file_name ("$EIFFEL_SRC\dotnet\helpers\isekey.snk")
 	use_all_cluster_name_as_namespace (no)
 	use_cluster_name_as_namespace (yes)
  	version ("2.0.1.1402")

cluster 
 
 	library base_net:		"$EIFFEL_SRC\library.net\base"
 		default
 			namespace ("EiffelSoftware.Library.BaseNet")
 		end

 	library base:		"$EIFFEL_SRC\library\base"
		exclude
 			"table_eiffel3"; "desc"; "classic";
 		default
			namespace ("EiffelSoftware.Library.Base")
 		end
 
assembly
	mscorlib: "$ISE_DOTNET_FRAMEWORK\mscorlib.dll"
	
	system_: "$ISE_DOTNET_FRAMEWORK\System.dll"
		prefix
			"SYSTEM_DLL_"
		end

	system_xml: "$ISE_DOTNET_FRAMEWORK\System.Xml.dll"
		prefix
			"XML_"
		end


end -- system EiffelSoftware.CodeDomBase

