system
	other_nested

root
	OTHER_NESTEDODBC (root_cluster): "make"

default
	assertion (all)
	multithreaded (no)
	console_application (yes)
	dynamic_runtime (no)
	dead_code_removal (yes)
	profile (no)
	line_generation (no)
	debug  (no)
	array_optimization (no)
	inlining (no)
	inlining_size ("4")
	precompiled ("$ISE_EIFFEL\precomp\spec\$ISE_PLATFORM\base");


cluster
	
	root_cluster: "$ISE_EIFFEL\examples\store\other_nested";

-- EiffelBase

	all base:			"$ISE_EIFFEL/library/base"
		exclude
			"desc";"table_eiffel3"
		end

	thread:						"$ISE_EIFFEL\library\thread"

	-- EiffelTime
	time:				"$ISE_EIFFEL\library\time";
	time_format(time):	"$\format";
	time_english(time):	"$\format\english";

	-- EiffelStore
	estore_support:		"$ISE_EIFFEL\library\store\support"
	estore_interface:	"$ISE_EIFFEL\library\store\interface"
	estore_db_support:	"$ISE_EIFFEL\library\store\dbms\support"
	estore_rdbms:		"$ISE_EIFFEL\library\store\dbms\rdbms\odbc"

external
	include_path:		
						"$(ISE_EIFFEL)\library\store\dbms\rdbms\odbc\Clib",
						"$ISE_EIFFEL\library\time\Clib"


	object: 			"$(ISE_EIFFEL)\library\store\spec\$(ISE_COMPILER)\lib\support.lib",
						"$(ISE_EIFFEL)\library\time\spec\$(ISE_COMPILER)\lib\datetime.lib",
						"$(ISE_EIFFEL)\library\store\spec\$(ISE_COMPILER)\lib\odbc_store.lib",
						"d:\odbcsdk\lib32\odbc32.lib"

end
