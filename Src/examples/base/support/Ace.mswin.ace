system
	demo_driver 

root

	demo_driver (ROOT_CLUSTER): "make"


default

	assertion (require);
	precompiled ("$EIFFEL4\precomp\spec\$PLATFORM\base")

cluster

	ROOT_CLUSTER: 		"$EIFFEL4\examples\base\support";
				-- If you intend to modify any part of the example,
				-- copy all the files of $EIFFEL4\examples\base\structures
				-- into one of your directories, and replace
				-- $EIFFEL4\examples\base\structures by the name of
				-- that directory on the above line.

		-- EiffelBase:
	all base:		"$EIFFEL4\library\base"
		exclude
			"desc";"table_eiffel3"
		end


end
