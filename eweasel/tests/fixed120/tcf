-- This is a test control file

test_name 	 whip-that-ace
test_description After initial compilation, some clusters are removed from the Ace and then added back to it

copy_sub 	 Ace $TEST Ace
copy_raw 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

copy_sub 	 Ace_rev1 $TEST Ace
compile_melted
if not DOTNET compile_result 	 validity_error ANY VTCT ; ANY VTCT ; ARRAY VTCT ; ARRAY VTCT ; ARRAY VTCT ; ARRAY VTCT ; COM_FAILURE VTCT ; COM_FAILURE VTCT ; EXCEPTION VTCT ; EXCEPTION_MANAGER VTCT ; EXCEPTION_MANAGER VTCT ; EXCEPTION_MANAGER VTCT ; FILE VTCT ; FILE VTCT ; FILE VTCT ; FILE VTCT ; FILE VTCT ; FILE VTCT ; INTEGER_INTERVAL VTCT ; INTEGER_INTERVAL VTCT ; INTEGER_INTERVAL VTCT ; INTEGER_INTERVAL VTCT ; IO_MEDIUM VTCT ; NUMERIC VTCT ; ROUTINE VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_GENERAL VTCT ; TUPLE VTCT ; TUPLE VTCT ; TUPLE VTCT ; TYPE VTCT
if DOTNET compile_result 	 validity_error ANY VTCT ; ANY VTCT ; ARRAY VTCT ; ARRAY VTCT ; ARRAY VTCT ; ARRAY VTCT ; COM_FAILURE VTCT ; COM_FAILURE VTCT ; DIRECTORY VTCT ; DIRECTORY VTCT ; DIRECTORY VTCT ; EXCEPTION VTCT ; EXCEPTION_MANAGER VTCT ; EXCEPTION_MANAGER VTCT ; FILE VTCT ; FILE VTCT ; FILE VTCT ; FILE VTCT ; FILE VTCT ; INTEGER_INTERVAL VTCT ; INTEGER_INTERVAL VTCT ; INTEGER_INTERVAL VTCT ; INTEGER_INTERVAL VTCT ; IO_MEDIUM VTCT ; NUMERIC VTCT ; RAW_FILE VTCT ; ROUTINE VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_32 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_8 VTCT ; STRING_GENERAL VTCT ; TUPLE VTCT ; TUPLE VTCT

copy_sub 	 Ace $TEST Ace
resume_compile
compile_result 	 ok

test_end
