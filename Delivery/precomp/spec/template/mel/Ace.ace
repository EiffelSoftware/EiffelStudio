system MEL

root

	ANY

default

	assertion (require);
	precompiled ("$EIFFEL4/precomp/spec/$PLATFORM/base")

cluster

-- MEL
	all mel:				"$EIFFEL4/library/mel/"

external
 
	include_path:
		"\$(EIFFEL4)/library/mel/Clib";
 
	object:
		"$(EIFFEL4)/library/mel/spec/$(PLATFORM)/lib/libMel.a",
		"$(EIFFEL4)/library/mel/spec/$(PLATFORM)/lib/libXpm.a",
		"-lXm -lXt -lX11"
 
end
