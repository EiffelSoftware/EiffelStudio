-- MEL
	mel:             "$EIFFEL4/library/mel";
	mel_callback_structs (mel):   "$/callback_structs";
	mel_constants (mel):          "$/constants";
	mel_events (mel):             "$/events";
	mel_extra (mel):              "$/extra";
