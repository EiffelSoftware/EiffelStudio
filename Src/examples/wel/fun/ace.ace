system
	fun_demo

root 
	FUN_DEMO (root_cluster): "make"

default
	assertion (all)

cluster 
	root_cluster:	"$ISE_EIFFEL\examples\wel\fun"

		-- EiffelBase
	all base: 		"$ISE_EIFFEL\library\base"
		exclude
			"table_eiffel3"
			"desc"
		end

		-- WEL
	all wel: 		"$ISE_EIFFEL\library\wel"
		exclude
			"spec"
			"clib"
		end

external
	include_path:	"$(ISE_EIFFEL)\library\wel\spec\windows\include"
	object: 		"$(ISE_EIFFEL)\library\wel\spec\$(ISE_C_COMPILER)\lib\wel.lib"

end -- fun_demo
