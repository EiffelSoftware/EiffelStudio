<?xml version="1.0" encoding="ISO-8859-1"?><system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="vision" uuid="ad30a056-8e73-403b-a474-a9bcc34bf63a" library_target="vision">
	<target name="vision">
		<description>EiffelVision Library: Platform independent graphical library of reusable components for Eiffel.
Copyright (c) 1984-2006, Eiffel Software and others.
Eiffel Forum License v2 (see http://www.eiffel.com/licensing/forum.txt).</description>
		<file_rule>
			<exclude>/\.svn$</exclude>
			<exclude>/EIFGEN.{0,1}$</exclude>
			<exclude>/temp$</exclude>
		</file_rule>
		<option>
			<assertions precondition="true" postcondition="false" check="false" invariant="false" loop="false"/>
		</option>
		<library name="base" location="$ISE_LIBRARY\library\base\base.ecf"/>
		<library name="mel" location="$ISE_LIBRARY\library\mel\mel.ecf">
			<condition>
				<platform excluded_value="windows"/>
			</condition>
		</library>
		<library name="wel" location="$ISE_LIBRARY\library\wel\wel.ecf">
			<condition>
				<platform value="windows"/>
			</condition>
		</library>
                <cluster name="vision" location="$ISE_LIBRARY/library/vision/">
                        <cluster name="vision_context_data" location="$|oui\context_data\"/>
                        <cluster name="vision_figures" location="$|figures\"/>
                        <cluster name="vision_imp_motif" location="$|implement\motif\">
                                <cluster name="vision_imp_kernel" location="$|kernel\"/>
                                <cluster name="vision_imp_support" location="$|support\"/>
                                <cluster name="vision_imp_widgets" location="$|widgets\"/>
				<condition>
					<platform excluded_value="windows"/>
				</condition>
                        </cluster>
                        <cluster name="vision_imp_x" location="$|implement\x\">
                                <cluster name="vision_imp_x_kernel" location="$|kernel\"/>
                                <cluster name="vision_imp_x_support" location="$|support\"/>
				<condition>
					<platform excluded_value="windows"/>
				</condition>
                        </cluster>
			<cluster name="vision_imp_mswin" location="$|implement\mswin\" recursive="true">
				<condition>
					<platform value="windows"/>
				</condition>
			</cluster>
                        <cluster name="vision_kernel" location="$|oui\kernel\"/>
                        <cluster name="vision_toolkit" location="$|implement\toolkit\">
                                <cluster name="vision_toolkit_kernel" location="$|kernel\"/>
                                <cluster name="vision_toolkit_support" location="$|support\"/>
                                <cluster name="vision_toolkit_widgets" location="$|widgets\"/>
                        </cluster>
                        <cluster name="vision_tools" location="$|tools\"/>
                        <cluster name="vision_widgets" location="$|oui\widgets\"/>
                </cluster>
	</target>
</system>
