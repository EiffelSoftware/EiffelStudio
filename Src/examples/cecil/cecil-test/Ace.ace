system tst
 
root
 
    main (root_cluster): "make"

default

	assertion (require);
	-- On windows, with ISE Compiler v4.3 : uncomment the next line
	-- console_application (yes); 
	-- Comment the next line, if you do not want to use 
	-- a precompiled Eiffel Base library.
	precompiled ("$EIFFEL4/precomp/spec/$PLATFORM/base")
 
cluster

		root_cluster: 			"$EIFFEL4/examples/cecil/cecil-test";
				-- If you intend to modify any part of the example,
				-- copy all the files of $EIFFEL4/examples/cecil/cecil-test
				-- into one of your directories, and replace
				-- $EIFFEL4/examples/cecil/cecil-test by the name of
				-- that directory on the above line.
			default
				assertion(no)
			visible
				MAIN 
			end
		end;   

		all base :			"$EIFFEL4/library/base"	
			visible
				STRING 
			end
			visible
				LINKED_LIST 
			end

end
