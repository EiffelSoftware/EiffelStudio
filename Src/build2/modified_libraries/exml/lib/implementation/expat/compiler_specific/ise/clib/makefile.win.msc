# exml.lib - Makefile for Microsoft C

CC = cl
CFLAGS = -c -Ox -I$(ISE_EIFFEL)\C_library\expat\xmlparse -I$(ISE_EIFFEL)\studio\spec\windows\include 
.c.obj:
	$(CC) $(CFLAGS) $<

OBJ = exml_parser.obj

exml.lib: $(OBJ)
	if exist $@ del $@
	lib /OUT:$@ $(OBJ)
	if not exist msc mkdir msc
	copy $@ msc
	del $@
	del *.obj
