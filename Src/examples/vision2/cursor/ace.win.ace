--!----------------------------------------------------------------
--! Project Ace-file for Windows platforms.
--!----------------------------------------------------------------

system
	cursor

root 
	CURSOR_TEST (root_cluster): "make_and_launch"

default
	assertion			(all)
	multithreaded		(no)
	console_application	(no)
	dynamic_runtime		(no)
	dead_code_removal		(yes)
	profile			(no)
	line_generation		(no)
	debug				(no)
	array_optimization	(no)
	inlining			(no)
	inlining_size		("4")

cluster 
		-- Root cluster
	root_cluster:		"$ISE_EIFFEL\examples\vision2\cursor"

		-- EiffelBase
	all base: 			"$ISE_EIFFEL\library\base"
		exclude
			"table_eiffel3"
		end

		-- WEL
	all wel:			"$ISE_EIFFEL\library\wel"

		-- Vision2
	all vision2:		"$ISE_EIFFEL\library\vision2"
		exclude 
			"gtk"
			"EIFGEN"
			"tmp"
			"temp"
			"release"
			"obsolete"
			"bench_used"
		end

external
	include_path:		"$(ISE_EIFFEL)\library\vision2\spec\include",
					"$(ISE_EIFFEL)\library\wel\spec\windows\include"

	object: 			"$(ISE_EIFFEL)\library\vision2\spec\$(ISE_C_COMPILER)\lib\vision2.lib",
					"$(ISE_EIFFEL)\library\wel\spec\$(ISE_C_COMPILER)\lib\wel.lib",
			 		"$(ISE_EIFFEL)\library\vision2\spec\$(ISE_C_COMPILER)\lib\libpng.lib",
			 		"$(ISE_EIFFEL)\library\vision2\spec\$(ISE_C_COMPILER)\lib\zlib.lib"

end -- cursor example
