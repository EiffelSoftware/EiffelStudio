system nest2

root  test_i: "make"


default

	assertion (all);
	precompiled ("$EIFFEL4/precomp/spec/$PLATFORM/base")

cluster

	root_cluster:		"f:/temp/examples/store/nesting2";
	estore_examples:	"f:/temp/examples/store/Utilities";
	odbc_handle:		"f:/temp/examples/store/Utilities/odbc"
		exclude 
			"action_1_i.e";"action_2_i.e"
		end

-- EiffelBase

	all base:			"$EIFFEL4/library/base"
		exclude
			"desc";"table_eiffel3"
		end
	
-- EiffelStore

	estore_support:		"$EIFFEL4/library/store/support";
	estore_interface:	"$EIFFEL4/library/store/interface";
	estore_odbc:		"$EIFFEL4/library/store/dbms/rdbms/odbc";
	estore_db_support:	"$EIFFEL4/library/store/dbms/support";

-- EiffelTime
	
	time:				"$EIFFEL4/library/time";
	time_format:		"$EIFFEL4/library/time/format";
	time_english:		"$EIFFEL4/library/time/format/english";
	
external

	include_path:
		"$EIFFEL4\library\store\dbms\rdbms\odbc\Clib";

	object:
		"$(EIFFEL4)/library/store/spec/$(COMPILER)/lib/support.lib",
		"$(EIFFEL4)/library/time/spec/$(COMPILER)/lib/datetime.lib",
		"$(EIFFEL4)/library/store/spec/$(COMPILER)/lib/odbc_store.lib",
		"odbc32.lib" -- Specify the correct path
end
