system
	tst 

root

	ROOT_CLASS (ROOT_CLUSTER): "make"

default

	assertion (require);
	console_application (yes);
	precompiled ("$EIFFEL4/precomp/spec/$PLATFORM/base")

cluster

	ROOT_CLUSTER: 		"$EIFFEL4/examples/Eiffel2C/object";
				-- If you intend to modify any part of the example,
				-- copy all the files of  $EIFFEL4/examples/Eiffel2C/object
				-- into one of your directories, and replace
				-- $EIFFEL4/examples/Eiffel2C/object by the name of
				-- that directory on the above line.

-- EiffelBase

    all base:           "$EIFFEL4/library/base"
		exclude
			"desc";"table_eiffel3"
		end

external

	--object: "../../fext.o";	-- On Unix
	object: "../../fext.obj";	-- On windows
	include_path: "../../.."
end
