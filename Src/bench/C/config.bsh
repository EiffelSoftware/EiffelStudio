link_mtline='&$compiler_path\bin\tlib /p256 $@ +-$**'
link_mtwline='&$compiler_path\bin\tlib /p256 $@ +-$**'
#
#
optimize='-O2'
debug=''
#optimize=''
#debug='-v'
#
cc='bcc32'
compiler_path='d:\apps\BCC55'
all_dependency=''
