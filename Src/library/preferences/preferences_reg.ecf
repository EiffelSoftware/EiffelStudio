<?xml version="1.0" encoding="ISO-8859-1"?><system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="preferences" uuid="9647aa82-399b-486f-9e41-5373590095b7" library_target="preferences">
	<target name="preferences">
		<description>Preferences library.
Copyright (c) 1984-2006, Eiffel Software and others.
Eiffel Forum License v2 (see http://www.eiffel.com/licensing/forum.txt).</description>
		<option>
			<assertions precondition="true" postcondition="false" check="false" invariant="false" loop="false"/>
		</option>
		<file_rule>
			<exclude>/\.svn</exclude>
		</file_rule>

		<library name="base" location="$ISE_LIBRARY\library\base\base.ecf" readonly="true"/>
		<library name="vision2" location="$ISE_LIBRARY\library\vision2\vision2.ecf" readonly="true">
			<condition><custom name="batch_preferences" excluded_value="true"/></condition>
		</library>
		<library name="wel" location="$ISE_LIBRARY\library\wel\wel.ecf" readonly="true"/>
		<library name="gobo" location="$ISE_LIBRARY\library\gobo\gobo.ecf" readonly="true"/>
		
		<cluster name="interface" location="interface" recursive="true">
			<condition><custom name="batch_preferences" excluded_value="true"/></condition>
		</cluster>
		<cluster name="preference" location="preference" recursive="true">
			<condition><custom name="batch_preferences" excluded_value="true"/></condition>
		</cluster>

		<cluster name="interface_batch" location="interface" recursive="true">
			<condition><custom name="batch_preferences" value="true"/></condition>
			<file_rule>
				<exclude>/graphical$</exclude>
				<exclude>/build_project$</exclude>
			</file_rule>
		</cluster>
		<cluster name="preference_batch" location="preference" recursive="true">
			<condition><custom name="batch_preferences" value="true"/></condition>
			<file_rule>
				<exclude>/graphical$</exclude>
			</file_rule>
		</cluster>
		<cluster name="storage" location="storage" recursive="true">
			<file_rule>
				<exclude>/xml_implementation$</exclude>
			</file_rule>
		</cluster>
		<cluster name="preferences" location="." recursive="false"/>
	</target>
</system>
