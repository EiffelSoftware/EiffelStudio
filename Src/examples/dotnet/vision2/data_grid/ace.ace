	"system_windows_forms":	"System.Windows.Forms","1.0.3300.0","neutral","b77a5c561934e089"
		prefix
			winforms_
		end

	"System_Data":	"System.Data","1.0.3300.0","neutral","b77a5c561934e089"
		prefix
			data_
		end

	"System_Web":	"System.Web","1.0.3300.0","neutral","b03f5f7f11d50a3a"
