system
	example

root

	example (ROOT_CLUSTER): "make"

system
	test_all_widgets

root
	DEMO (root_cluster): "make"

default
	assertion (no)
	precompiled ("$ISE_EIFFEL\precomp\spec\$ISE_PLATFORM\wel")

cluster
	root_cluster:	"$ISE_EIFFEL/examples/vision/simple_test"

		-- EiffelVision
    all vision:        	"$ISE_EIFFEL\library\vision"
		exclude
			"motif"
			"x"
			"gtk"
		end

external
	include_path:
		"$(ISE_EIFFEL)\library\wel\spec\windows\include";

	object:
		"$(ISE_EIFFEL)\library\wel\spec\$(ISE_COMPILER)\lib\wel.lib";

end
