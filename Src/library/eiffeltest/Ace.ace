system
	eiffeltest

root
	ANY

default
	assertion (all)
	precompiled ("$EIFFEL4/precomp/spec/$PLATFORM/base")

cluster

	all library:	"library"
		exclude
			"CVS"
		end


	-- EiffelBase
	
	all base:		"$EIFFEL4/library/base"
		exclude
			"desc"; "table_eiffel3"
		end

	
	-- EiffelTime

	all time:	"$EIFFEL4/library/time"
		exclude
			"french"; "german"
		end

end -- system eiffeltest
