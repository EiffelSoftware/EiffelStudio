		<PREF NAME="editor.mouse_wheel_scroll_size" DESCRIPTION="Mouse wheel scroll size"><INTEGER>3</INTEGER></PREF>
		<PREF NAME="editor.remove_trailing_white_space" DESCRIPTION="Automatically remove trailing white space from lines?"><BOOLEAN>True</BOOLEAN></PREF>
		<PREF NAME="editor.blinking_cursor" DESCRIPTION="Does text cursor blink?"><BOOLEAN>True</BOOLEAN></PREF>
		<PREF NAME="editor.automatic_update" DESCRIPTION="Automatic update when the file has not been modified"><BOOLEAN>True</BOOLEAN></PREF>
