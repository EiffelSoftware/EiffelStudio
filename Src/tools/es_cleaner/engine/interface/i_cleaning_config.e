indexing
	description: "[
		A full interface to expose configuration information to a cleaner.
	]"
	legal: "See notice at end of class."
	status: "See notice at end of class.";
	date: "$date$";
	revision: "$revision$"

deferred class
	I_CLEANING_CONFIG

inherit
	I_PROJECT_CONFIG
		rename
			can_any_processing_occur as can_project_processing_occur
		end

	I_ENVIRONMENT_CONFIG
		rename
			can_any_processing_occur as can_environment_processing_occur
		end

feature -- Status report

	can_any_processing_occur: BOOLEAN is
			-- Indiciates if any processing can occur
		do
			Result := can_project_processing_occur or can_environment_processing_occur
		end

