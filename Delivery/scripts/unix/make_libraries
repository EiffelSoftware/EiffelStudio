#!/bin/sh

remtrace Retrieve libraries
cd $INSTALL_DIR
md library
cd library
remtrace base
exprt -r $DEFAULT_CVS_TAG -d base Src/library/base
remtrace Eiffel2Java
exprt -r $DEFAULT_CVS_TAG -d Eiffel2Java Src/library/Eiffel2Java
remtrace event
exprt -r $DEFAULT_CVS_TAG -d event Src/library/event
remtrace gel
exprt -r $DEFAULT_CVS_TAG -d gel Src/library/gel
remtrace lex
exprt -r $DEFAULT_CVS_TAG -d lex Src/library/lex
remtrace mel
exprt -r $DEFAULT_CVS_TAG -d mel Src/library/mel
remtrace net
exprt -r $DEFAULT_CVS_TAG -d net Src/library/net
remtrace parse
exprt -r $DEFAULT_CVS_TAG -d parse Src/library/parse
remtrace store
exprt -r $DEFAULT_CVS_TAG -d store Src/library/store
remtrace thread
exprt -r $DEFAULT_CVS_TAG -d thread Src/library/thread
remtrace time
exprt -r $DEFAULT_CVS_TAG -d time Src/library/time
remtrace vision
exprt -r $DEFAULT_CVS_TAG -d vision Src/library/vision
remtrace vision2
exprt -r $DEFAULT_CVS_TAG -d vision2 Src/library/vision2
fullrd vision2/implementation/mswin
remtrace web
exprt -r $DEFAULT_CVS_TAG -d web Src/library/web

cd ../..

remtrace Retrieve examples
cd $INSTALL_DIR
md examples
cd examples
remtrace base
exprt -r $DEFAULT_CVS_TAG -d base Src/examples/base
remtrace Eiffel2Java
exprt -r $DEFAULT_CVS_TAG -d Eiffel2Java Src/examples/Eiffel2Java
remtrace cecil
exprt -r $DEFAULT_CVS_TAG -d cecil Src/examples/cecil
remtrace cpp
exprt -r $DEFAULT_CVS_TAG -d cpp Src/examples/cpp
remtrace lex
exprt -r $DEFAULT_CVS_TAG -d lex Src/examples/lex
remtrace mel
exprt -r $DEFAULT_CVS_TAG -d mel Src/examples/mel
remtrace net
exprt -r $DEFAULT_CVS_TAG -d net Src/examples/net
remtrace parse
exprt -r $DEFAULT_CVS_TAG -d parse Src/examples/parse
remtrace store
exprt -r $DEFAULT_CVS_TAG -d store Src/examples/store
fullrd store/matisse
remtrace thread
exprt -r $DEFAULT_CVS_TAG -d thread Src/examples/thread
remtrace time
exprt -r $DEFAULT_CVS_TAG -d time Src/examples/time
remtrace vision
exprt -r $DEFAULT_CVS_TAG -d vision Src/examples/vision
remtrace vision2
exprt -r $DEFAULT_CVS_TAG -d vision2 Src/examples/vision2
cd vision2
fullrd Boxes
fullrd drawing_area
fullrd ev_list
fullrd fake_event
fullrd figure
fullrd hello_world
fullrd menu
fullrd multicolumn_list
fullrd pixmap
fullrd split_area
fullrd test_all_widgets
fullrd test_events
fullrd tests
fullrd tutorial
cd ..
remtrace studio
exprt -r $DEFAULT_CVS_TAG -d studio Src/examples/studio

cd ../..
