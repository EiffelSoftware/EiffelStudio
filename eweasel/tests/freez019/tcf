-- This is a test control file

test_name 	tuple-i16-plus-i8
test_description A class which prints `<< i16 + i8, i >> @ 1' prints -32642 when code is melted, but 32894 when frozen.


copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
compile_melted
compile_result 	ok

execute_work 	NONE exec_output1
execute_result 	ok
compare		exec_output1 output

compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work 	NONE exec_output2
execute_result 	ok
compare		exec_output2 output

test_end
