system runner

root
	
	runner_ing (root_cluster): "make"

default

	assertion (all);
	precompiled ("$ISE_EIFFEL/precomp/spec/$ISE_PLATFORM/base")

cluster

	root_cluster:		".";
	estore_examples_objects:"$ISE_EIFFEL/library/store/examples/Utilities";
	ingres_handle:		"$ISE_EIFFEL/library/store/examples/Utilities/ingres";

-- EiffelBase

	all base:			"$ISE_EIFFEL/library/base"
		exclude
			"desc";"table_eiffel3"
		end

-- EiffelStore

	estore_support:		"$ISE_EIFFEL/library/store/support";
	estore_interface:	"$ISE_EIFFEL/library/store/interface";
	estore_date:		"$ISE_EIFFEL/library/store/date_and_time";
	estore_db_support:	"$ISE_EIFFEL/library/store/dbms/support";
	estore_ingres:		"$ISE_EIFFEL/library/store/dbms/rdbms/ingres";

external

	object: "-L$(ISE_EIFFEL)/library/store/spec/$(ISE_PLATFORM)/lib";
		"-lsys_time";
		"-lsupport";
		"-lingres_store";
		"-L$(II_SYSTEM)/ingres/lib"; 
		"-lingres"

end
