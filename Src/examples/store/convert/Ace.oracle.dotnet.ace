system
	"converter"

root
	parser (root_cluster): "make"

default
	assertion (require)
	msil_generation (yes)
	il_verifiable (yes)
	cls_compliant (yes)
	use_cluster_name_as_namespace (yes)
	use_all_cluster_name_as_namespace (yes)
	msil_generation_type ("exe")
	check_vape (yes)
	console_application (yes)

cluster
	root_cluster:		"$ISE_LIBRARY\examples\store\convert"

	estore_examples_objects:	"$ISE_LIBRARY\examples\store\Utilities";
	oracle_handle:			"$ISE_LIBRARY\examples\store\Utilities\oracle";

	estore_support:		"$ISE_LIBRARY\library\store\support";
	estore_interface:		"$ISE_LIBRARY\library\store\interface";
	estore_convert:		"$ISE_LIBRARY\library\store\converter";
	estore_db_support:	"$ISE_LIBRARY\library\store\dbms\support";
	estore_oracle:		"$ISE_LIBRARY\library\store\dbms\rdbms\oracle";
	all generation:		"$ISE_LIBRARY\library\store\generation";

	time:						"$ISE_LIBRARY\library\time";
	time_format(time):				"$\format";
	english_format(time_format):	"$\english";

	lexical:			"$ISE_LIBRARY\library\lex";

	all base:		"$ISE_LIBRARY\library\base"
		exclude
			"table_eiffel3"; "desc"; "classic"; 
		end

	all base_net:		"$ISE_LIBRARY\library.net\base"

assembly
	mscorlib:	"$ISE_DOTNET_FRAMEWORK\mscorlib.dll"

	system_:	"$ISE_DOTNET_FRAMEWORK\System.dll"
		prefix
			"SYSTEM_DLL_"
		end

	system_xml:	"$ISE_DOTNET_FRAMEWORK\System.Xml.dll"
		prefix
			"XML_"
		end

external
	include_path: 
		"$(ISE_LIBRARY)\library\store\dbms\rdbms\oracle\Clib",
-- Insert here the path which contains the Oracle OCI
		"$(ORACLE_HOME)\OCI\include"

	object:
		"$(ISE_LIBRARY)\library\store\spec\$(ISE_C_COMPILER)\lib\oracle_store.lib",
-- Insert here the file oci.lib
		"$(ORACLE_HOME)\OCI\lib\msvc\oci.lib"

end