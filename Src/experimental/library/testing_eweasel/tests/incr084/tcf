-- This is a test control file

test_name 	 chg-bit-length-refreeze
test_description After initial compilation where the system is frozen, the value of an inherited constant INTEGER attribute is changed and the system is refrozen and re-executed

copy_sub 	 Ace $TEST Ace
define		 LENGTH "13"
copy_raw 	 test.e  $CLUSTER test.e
copy_sub 	 test1.e  $CLUSTER test1.e
compile_frozen
compile_result 	 ok

c_compile_work
c_compile_result ok

execute_work	NONE exec_output1
execute_result	ok
compare		exec_output1 output1

define		 LENGTH "8"
copy_sub 	 test1.e  $CLUSTER test1.e
compile_frozen
compile_result 	 ok

c_compile_work
c_compile_result ok

execute_work	NONE exec_output2
execute_result	ok
compare		exec_output2 output2

test_end
