#!/bin/sh

remtrace Compile the Eiffel libraries C code
if [ -d $1/library/cURL ]; then
	remtrace cURL
	cd $1/library/cURL/Clib
	default_make
fi 
remtrace net
cd $1/library/net/Clib
default_make
mac_ranlib $1/library/net/spec/$ISE_PLATFORM/lib/libnet.a
remtrace obsolete/net
cd $1/library/obsolete/net/Clib
default_make
mac_ranlib $1/library/obsolete/net/spec/$ISE_PLATFORM/lib/libnet.a
remtrace vision2
cd $1/library/vision2/Clib
default_make
cd $1/library/vision2/implementation/gtk/Clib
default_make
cd $1
