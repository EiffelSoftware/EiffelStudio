      <form action="details.xeb" method="post" id="form1">
        <table width="100%" border="0" cellpadding="0">
          <tr>
            <th scope="row"><div align="left">Id</div></th>
            <td><div align="left">
                <xeb:if condition="not_authenticated_admin">
                <xeb:display feature="get_res_id_from_args"/>
                </xeb:if>
                <xeb:if condition="authenticated_admin">
                <input name="id" type="text" id="id" value="%=get_res_id_from_args%" />
                </xeb:if>
              </div></td>
          </tr>
          <tr>
            <th scope="row"><div align="left">Name</div></th>
            <td><div align="left">
                <xeb:if condition="not_authenticated_admin">
                <xeb:display feature="get_res_name_from_args"/>
