system
	store_wizard

root
	WIZARD_PROJECT_MANAGER: make_and_launch

default
	assertion(require)
	assertion(ensure)
	assertion(check)
	address_expression(no)
	array_optimization(no)
	console_application(no)
	dead_code_removal(yes)
	dynamic_runtime(no)
	exception_trace(yes)
	inlining(no)
	multithreaded(no)
	check_vape(no)
	arguments("D:\help\wizards\estore2")
	arguments("D:\help\wizards\estore2\resources")
	arguments(" ")
	disabled_debug(yes)
	disabled_debug("wel")
	disabled_debug("win_dispatcher")
	disabled_debug("gdi_count")
	disabled_debug("vision2_windows_gdi")
	disabled_debug("vision2_windows")
	disabled_debug("wel_gdi_count")
	disabled_debug("wel_gdi_references")
	disabled_debug("dlg_dispatcher")
	debug(no)
	line_generation(no)
	profile(no)
	trace(no)

cluster
	root_cluster:		"D:\help\wizards\estore2\src"

	all base:		"D:\Eiffel_Projects\base"
		exclude
			"desc"; "table_eiffel3"; 
		end

	all vision2:		"$EIFFEL4\library\vision2"
		exclude
			"gtk"; "test"; 
		end

	all wel:		"$EIFFEL4\library\wel"

	all wizard_tool:		"$EIFFEL4\library\wizard"
		exclude
			"CVS"; "unix"; 
		end

	time:		"$EIFFEL4\library\time"

	time_format (time):		"$\format"

	time_english (time):		"$\format\english"

	estore_support:		"$EIFFEL4\library\store\support"

	estore_interface:		"$EIFFEL4\library\store\interface"

	estore_db_support:		"$EIFFEL4\library\store\dbms\support"

	all estore_rdbms:		"$EIFFEL4\library\store\dbms\rdbms"

	all estore_generation:		"$EIFFEL4\library\store\generation"

external

	include_path:
		"$EIFFEL4\library\wel\spec\windows\include",
		"$EIFFEL4\library\store\dbms\rdbms\oracle\Clib",
		"$(ORACLE_HOME)\OCI\include",
		"$EIFFEL4\library\store\dbms\rdbms\odbc\Clib",
		"$EIFFEL4\library\vision2\spec\include",
		"$EIFFEL4\library\time\Clib",
		"$EIFFEL4\library\wizard\help\Clib"

	object:
		"$EIFFEL4\library\store\spec\$(COMPILER)\lib\support.lib",
		"$EIFFEL4\library\store\spec\$(COMPILER)\lib\oracle_store.lib",
		"$EIFFEL4\library\store\spec\$(COMPILER)\lib\odbc32.lib",
		"$EIFFEL4\library\store\spec\$(COMPILER)\lib\odbc_store.lib",
		"$EIFFEL4\library\wel\spec\msc\lib\wel.lib",
		"$EIFFEL4\library\vision2\spec\$(COMPILER)\lib\vision2.lib",
		"$EIFFEL4\library\vision2\spec\$(COMPILER)\lib\libpng.lib",
		"$EIFFEL4\library\vision2\spec\$(COMPILER)\lib\zlib.lib",
		"$EIFFEL4\library\time\spec\$(COMPILER)\lib\datetime.lib",
		"$(ORACLE_HOME)\OCI\lib\msvc\oci.lib",
		"$EIFFEL4\library\wizard\help\Clib\htmlhelp.lib"

end
