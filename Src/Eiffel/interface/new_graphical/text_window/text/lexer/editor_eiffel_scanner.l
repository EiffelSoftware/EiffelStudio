----------/** Separators **/----------------------------------------------------

[ ]+				{
					curr_token := new_space (text_count)
					update_token_list
					}
[\t]+				{
					if not in_comments then
						curr_token := new_tabulation (text_count)
					else
						curr_token := new_comment
					end
					update_token_list
					}
\r\n				{
					curr_token := new_eol (True)
					update_token_list
					in_comments := False
					}
\n					{
					curr_token := new_eol (False)
					update_token_list
					in_comments := False
					}
\r					{
					curr_token := new_text
					update_token_list
					}


