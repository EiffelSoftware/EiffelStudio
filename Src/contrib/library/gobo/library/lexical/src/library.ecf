<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-19-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-19-0 http://www.eiffel.com/developers/xml/configuration-1-19-0.xsd" name="lexical" uuid="E1FFE100-60B0-0001-0013-001000000001" library_target="lexical">
	<description>
		description: "ECF file for the Gobo Eiffel Lexical Library"
		library: "Gobo Eiffel Lexical Library"
		copyright: "Copyright (c) 2001-2018, Eric Bezault and others"
		license: "MIT License"
	</description>
	<target name="lexical">
		<root all_classes="true"/>
		<file_rule>
			<exclude>/EIFGENs$</exclude>
			<exclude>/\.git$</exclude>
			<exclude>/\.svn$</exclude>
		</file_rule>
		<option warning="true">
		</option>
		<variable name="GOBO_LIBRARY" value="../../.."/>
		<library name="free_elks" location="${GOBO_LIBRARY}\library\free_elks\library_${GOBO_EIFFEL}.ecf" readonly="true"/>
		<library name="kernel" location="${GOBO_LIBRARY}\library\kernel\library.ecf" readonly="true"/>
		<library name="lexical_skl" location="${GOBO_LIBRARY}\library\lexical\library_skl.ecf" readonly="true"/>
		<library name="parse_skl" location="${GOBO_LIBRARY}\library\parse\library_skl.ecf" readonly="true"/>
		<library name="pattern" location="${GOBO_LIBRARY}\library\pattern\library.ecf" readonly="true"/>
		<library name="regexp" location="${GOBO_LIBRARY}\library\regexp\library.ecf" readonly="true"/>
		<library name="structure" location="${GOBO_LIBRARY}\library\structure\library.ecf" readonly="true"/>
		<library name="utility" location="${GOBO_LIBRARY}\library\utility\library.ecf" readonly="true"/>
		<cluster name="lexical" location=".\" recursive="true">
			<file_rule>
				<exclude>/skeleton$</exclude>
			</file_rule>
		</cluster>
	</target>
</system>
