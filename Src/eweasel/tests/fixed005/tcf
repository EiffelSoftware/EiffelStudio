-- This is a test control file

test_name 	 syntax-wrong-positions
test_description Check positions for syntax error when manifest strings precede the position where the error is
copy_sub 	 Ace $TEST Ace
copy_raw 	 test.e  $CLUSTER test.e
copy_raw 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 syntax_error TEST1 25
test_end
