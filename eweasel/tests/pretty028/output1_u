class
	TEST

create
	make

feature -- Routines
-- This is a comment before first routine in section

-- This is a separate comment before first routine in section

	make
		do
		end

	try
		do
		end

-- This is comment 1
-- This is part of comment 1

-- This is a separate comment

	try
		do
		end --try

-- Another separate comment

	try2
		do
		end --try2
-- With a trailing comment

	try3
		do
		end
-- With a trailing comment only

	x: INTEGER
			-- Value

-- This is a comment between `x' and `y'

	y: INTEGER = 47
			-- Default value for `x'

-- This is a separate comment after the comment for `y'

end
