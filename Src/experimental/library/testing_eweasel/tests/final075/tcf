-- This is a test control file

test_name 	finalize-external-creation-procedure
test_description An external creation procedure is not called when the system is finalized

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
copy_raw 	parent.e $CLUSTER parent.e
copy_raw 	child.e $CLUSTER child.e
compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work 	NONE exec_output1
execute_result 	ok
compare		exec_output1 output

compile_final
compile_result 	ok

c_compile_final
c_compile_result ok

execute_final 	NONE exec_output2
execute_result 	ok
compare		exec_output2 output

test_end
