system
	h2e

root 
	H2E (root_cluster): "make"

default
	assertion (all)
	precompiled ("$ISE_EIFFEL\precomp\spec\$ISE_PLATFORM\wel")

cluster 
	root_cluster:	"."

		-- EiffelBase
	all base: 		"ISE_EIFFEL\library\base"
		exclude
			"table_eiffel3"
		end

		-- WEL
	all wel:		"ISE_EIFFEL\library\wel"

external
	include_path:	"$(ISE_EIFFEL)\library\wel\spec\$(ISE_PLATFORM)\include"
	object: 		"$(ISE_EIFFEL)\library\wel\spec\$(ISE_C_COMPILER)\lib\wel.lib"

end -- system h2e
