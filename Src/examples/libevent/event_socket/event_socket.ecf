		<library name="net" location="$ISE_LIBRARY\library\net\net-safe.ecf" readonly="false"/>
		<cluster name="event_socket" location=".\" recursive="true">
			<file_rule>
				<exclude>/EIFGENs$</exclude>
				<exclude>/CVS$</exclude>
				<exclude>/.svn$</exclude>
			</file_rule>
		</cluster>
	</target>
</system>
