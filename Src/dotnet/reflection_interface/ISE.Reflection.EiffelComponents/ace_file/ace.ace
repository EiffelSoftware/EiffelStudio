system
	"ISE.Reflection.EiffelComponents"

root
	EIFFEL_COMPONENTS (root_cluster): "make"

default
	assertion (all)
	line_generation (no)
	msil_generation (yes)
	msil_generation_type ("dll")

cluster	
	root_cluster: "$EIFFEL_SRC\dotnet\reflection_interface\ISE.Reflection.EiffelComponents"
		exclude
			"CVS";
		end

	all dot_net_base: "$EIFFEL_SRC\library.net\base"	
		exclude
			"CVS";
		end
end
