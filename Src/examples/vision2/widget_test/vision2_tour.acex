<?xml version="1.0" encoding="ISO-8859-1"?>
<system
	 xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd"
	 name="vision2_tour" uuid="3136A08C-7C74-4922-9026-81A822D3FE13">
	<target name="vision2_tour">
		<root feature="make_and_launch" class="VISION2_APPLICATION"/>
		<file_rule>
			<exclude>/.svn$</exclude>
		</file_rule>
		<option trace="false" profile="false" debug="false">
			<assertions precondition="true" postcondition="true" check="true" invariant="false" loop="false"/>
		</option>
		<setting name="console_application" value="False"/>
		<setting name="dead_code_removal" value="True"/>
		<setting name="exception_trace" value="True"/>
		<setting name="inlining" value="true"/>
		<setting name="inlining_size" value="0"/>

		<library name="base" location="$ISE_LIBRARY/library/base/base.acex"/>
		<library name="vision2" location="$ISE_LIBRARY/library/vision2/vision2.acex"/>
		<cluster name="root_cluster" location="." recursive="true">
			<file_rule>
				<exclude>/templates$</exclude>
				<exclude>/EIFGENs$</exclude>
			</file_rule>
		</cluster>
	</target>
</system>
