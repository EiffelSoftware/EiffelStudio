<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-15-0" name="client">
	<target name="client">
		<root class="CLIENT" feature="make"/>
		<option void_safety="none">
		</option>
		<setting name="console_application" value="false"/>
		<setting name="total_order_on_reals" value="true"/>
		<setting name="concurrency" value="thread"/>
		<precompile name="precompile" location="$PRECOMPILED_BASE_MT"/>
		$EWEASEL_DOTNET_SETTING
		<library name="base" location="$ISE_LIBRARY\library\base\base.ecf"/>
		<library name="process" location="$ISE_LIBRARY\library\process\process.ecf"/>
		<cluster name="client" location="$CLUSTER\"/>
	</target>
</system>
