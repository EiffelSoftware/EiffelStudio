system
	eiffeltest

root
	ANY

default
	assertion (all)
	precompiled ("$ISE_EIFFEL\precomp\spec\$ISE_PLATFORM\base")

cluster

	all eiffeltest:	"library"
		exclude
			"CVS"
		end

		-- EiffelBase
	all base:		"$ISE_EIFFEL\library\base"
		exclude
			"desc"; "table_eiffel3"
		end

	
		-- EiffelTime
	all time:	"$ISE_EIFFEL\library\time"
		exclude
			"french"; "german"
		end

end -- system eiffeltest
