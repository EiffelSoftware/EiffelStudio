			chk_project_session.set_minimum_height (19)
			set_padding (4)
			
			set_all_attributes_using_constants
			
				-- Connect events.
			chk_project.select_actions.extend (agent on_project_changed)
			txt_project_path.change_actions.extend (agent on_project_path_text_changed)
			btn_browse_project.select_actions.extend (agent on_project_browse_clicked)
			chk_project_layout.select_actions.extend (agent on_project_layout_changed)
			chk_project_preferences.select_actions.extend (agent on_project_preferences_changed)
			chk_project_session.select_actions.extend (agent on_project_session_changed)
				-- Close the application when an interface close
				-- request is recieved on `Current'. i.e. the cross is clicked.

				-- Call `user_initialization'.
			user_initialization
