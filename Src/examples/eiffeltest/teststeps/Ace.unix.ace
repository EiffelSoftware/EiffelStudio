system
	seqtest

root
	TEST_ROOT(root_cluster): "make"

default
	assertion (all)
	precompiled ("$EIFFEL4/precomp/spec/$PLATFORM/base")

cluster
	root_cluster:	"."

	all time:	"$EIFFEL4/library/time"
		exclude
			"german"; "french"
		end

	all eiffeltest: "$EIFFEL4/library/eiffeltest"

external
	object:
		"\$(EIFFEL4)/library/time/Clib/spec/\$(PLATFORM)/lib/libdatetime.a"

end
