<?xml version="1.0"?>

<project name="support" default="help">

	<description>
		system: "Gobo Eiffel Utility Library Unit Tester"
		copyright: "Copyright (c) 2017, Eric Bezault and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/library/common/config/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system" export="NONE">
		<set name="system" value="xsupport"/>
		<set name="library" value="utility"/>
		<set name="system_dir" value="${GOBO}/library/${library}/test/unit/support"/>
	</target>

</project>
