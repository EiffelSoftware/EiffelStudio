-- This is a test control file

test_name 	 validity-vrfa-inherited
test_description Violate validity constraint VRFA with a feature which has an argument with the same name as an inherited feature

copy_sub 	 Ace $TEST Ace

define		 ARGUMENTS "weasel: INTEGER"
define		 INHERITANCE "inherit TEST1"
define		 FEATURE ""
define		 TEST1_FEATURE "weasel is do end"
copy_sub 	 test.e  $CLUSTER test.e
copy_sub 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 validity_error TEST VRFA

define		 FEATURE "weasel is do end"
define		 TEST1_FEATURE ""
copy_sub 	 test.e  $CLUSTER test.e
copy_sub 	 test1.e  $CLUSTER test1.e
resume_compile
compile_result 	 validity_error TEST VRFA

define		 FEATURE ""
copy_sub 	 test.e  $CLUSTER test.e
resume_compile
compile_result 	 ok

define		 ARGUMENTS "weasel: INTEGER"
define		 INHERITANCE "inherit TEST1"
define		 FEATURE ""
define		 TEST1_FEATURE "weasel is do end"
copy_sub 	 test.e  $CLUSTER test.e
copy_sub 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 validity_error TEST VRFA

define		 INHERITANCE ""
copy_sub 	 test.e  $CLUSTER test.e
resume_compile
compile_result 	 ok

define		 ARGUMENTS "weasel: INTEGER"
define		 INHERITANCE "inherit TEST1"
define		 FEATURE ""
define		 TEST1_FEATURE "weasel is do end"
copy_sub 	 test.e  $CLUSTER test.e
copy_sub 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 validity_error TEST VRFA

define		 INHERITANCE "inherit TEST1 rename weasel as wimp end"
copy_sub 	 test.e  $CLUSTER test.e
resume_compile
compile_result 	 ok

define		 INHERITANCE "inherit TEST1 rename infix "@weasel" as wimp end"
copy_sub 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 validity_error TEST VRFA

define		 ARGUMENTS "weasel: INTEGER"
define		 INHERITANCE "inherit TEST1"
define		 FEATURE ""
define		 TEST1_FEATURE "weasel is do end"
copy_sub 	 test.e  $CLUSTER test.e
copy_sub 	 test1.e  $CLUSTER test1.e
resume_compile
compile_result 	 validity_error TEST VRFA

define		 TEST1_FEATURE "prefix "@weasel": REAL is do end"
copy_sub 	 test1.e  $CLUSTER test1.e
resume_compile
compile_result 	 ok

test_end
