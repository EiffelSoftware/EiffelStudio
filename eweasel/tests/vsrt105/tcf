-- This is a test control file

test_name 	 validity-vsrt1-new-root
test_description Violate validity constraint VSRT1 by changing the name of the root class in the Ace to a generic class
define		 ROOT_CLASS TEST
copy_sub 	 Ace $TEST Ace
copy_sub 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

define		 ROOT_CLASS TEST1
copy_sub 	 Ace $TEST Ace
copy_sub 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 validity_error TEST1 VSRT(1)

test_end
