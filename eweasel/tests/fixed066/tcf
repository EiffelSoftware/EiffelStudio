-- This is a test control file

test_name 	 non-boolean-exit
test_description Change the type of an argumentless function from BOOLEAN to INTEGER, thereby violating validity constraint VWBE

copy_sub 	 Ace $TEST Ace
copy_raw 	 test.e  $CLUSTER test.e
define		 ATTRIBUTE "attribute: BOOLEAN is do end"
copy_sub 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 ok

define		 ATTRIBUTE "attribute: INTEGER is do end"
copy_sub 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 validity_error TEST VWBE

define		 ATTRIBUTE "attribute: INTEGER is external "C" end"
copy_sub 	 test1.e  $CLUSTER test1.e
resume_compile
compile_result 	 validity_error TEST VWBE

define		 ATTRIBUTE "attribute: BOOLEAN is external "C" end"
copy_sub 	 test1.e  $CLUSTER test1.e
resume_compile
compile_result 	 ok

test_end
