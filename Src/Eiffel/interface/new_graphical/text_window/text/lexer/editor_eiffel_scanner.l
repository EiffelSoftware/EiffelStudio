----------/** Eiffel identifiers **/--------------------------------------------

[A-Z][A-Z0-9_]*					{
							if not in_comments then
								if is_current_group_valid then
									tmp_classes := current_group.class_by_name (unicode_text, True)
									if not tmp_classes.is_empty then
										curr_token := new_class
										curr_token.set_pebble (stone_of_class (tmp_classes.first))
									else
										curr_token := new_text
									end
								else
									curr_token := new_text
								end							
							else
								curr_token := new_comment
							end
							update_token_list
						}


{A}{X}*						{
							if not in_comments then
								curr_token := new_text											
							else
								curr_token := new_comment
							end
							update_token_list
						}


----------/** Eiffel free operators **/-----------------------------------------

{OLD_FREE_OPERATOR}	|
{FOF}{FOM}*({FOL}{-}[\-]\-|{FOL}{-}[\->]>|{FOL}{-}[<]<)?	|
{FOF}{FOM}*{FOL}{-}[\-]\-/{BREAK}	|
{FOF}{FOM}*{FOL}{-}[\->]>/{SOFT_BREAK}	|
{FOF}{FOM}*{FOL}{-}[<]</{SOFT_BREAK}	|
{FOF}{FOM}*/{BREAK}	|
{FOF}{FOM}*({FOL}{-}[\-<>]|\-{FOL}{-}[\->]|>{FOL}{-}[\->]|<{FOL}{-}[\-<])/{SOFT_BREAK}	|
{FOF}/{SOFT_BREAK}	{
										if not in_comments then
											curr_token := new_operator
										else
											curr_token := new_comment
										end
										update_token_list
										}
	-- Note: Accepts non-printable characters as well,
	-- provided that they are not break characters.


----------/** Eiffel characters **/---------------------------------------------

\'[^%\n']\'	|
\'%A\'		|
\'%B\'		|
\'%C\'		|
\'%D\'		|
\'%F\'		|
\'%H\'		|
\'%L\'		|
\'%N\'		|
\'%Q\'		|
\'%R\'		|
\'%S\'		|
\'%T\'		|
\'%U\'		|
\'%V\'		|
\'%%\'		|
\'%\'\'		|
\'%\"\'		|
\'%\(\'		|
\'%\)\'		|
\'%<\'		|
\'%>\'		|
\'%\/{I}\/\'	|
\'%\/0[xX]{H}(({H}|_)*{H})?\/\' |
\'%\/0[cC]{O}(({O}|_)*{O})?\/\' |
\'%\/0[bB]{B}(({B}|_)*{B})?\/\' {
		if not in_comments then
			curr_token := new_character
		else
			curr_token := new_comment
		end
		update_token_list
	}

\'.{0,2}		|
\'%\/[0-9]+(\/)?	{
			-- Character error. Catch-all rules (no backing up)
		if not in_comments then
			curr_token := new_text
		else
			curr_token := new_comment
		end
		update_token_list
	}


----------/** Eiffel strings **/------------------------------------------------

-- We accept here a fully formed Eiffel string which is complete, i.e:
-- 1 - whatever but `"' and '\n' except '%"' which is enclosed in
--     between (","), (",%), (%,%) or (%,")
-- We also accept beginning of a string, i.e:
-- 2 - whatever that start with either `"' or `%' and that does
--     not finishes with a `"' or `%'. Because we needed to support
--     the special character such as %N, %T,... we have to list all of
--     them in the second expression.

 \"[^\n"%]*[\[\{]/[ \t\r]* {
 		if not in_comments then
				-- Verbatim string opener.
			curr_token := new_string
			update_token_list
			in_verbatim_string := True
			start_of_verbatim_string := True
			set_start_condition (VERBATIM_STR1)
		else
			curr_token := new_comment
			update_token_list
		end
	}

<VERBATIM_STR1>{
	[ \t\r]*[\]\}][^%\n"]*\" {
				-- Verbatim string closer, possibly.
			curr_token := new_string
			end_of_verbatim_string := True
			in_verbatim_string := False
			set_start_condition (INITIAL)
			update_token_list
		}
	[ ]+ {
			curr_token := new_space (text_count)
			update_token_list						
		}
					
	[\t]+ {
			curr_token := new_tabulation (text_count)
			update_token_list						
		}
					
	\r\n {
			curr_token := new_eol (True)
			update_token_list
			in_comments := False
		}
	\n {
			curr_token := new_eol (False)
			update_token_list
			in_comments := False
		}
	\r {
			curr_token := new_text
			update_token_list
		}
	\" {
			curr_token := new_string
			update_token_list
		}
	[^\n\ \t\"]* {
			curr_token := new_string
			update_token_list
		}
	}

[%\"]([^\n\"%]|%\"|%A|%B|%C|%D|%F|%H|%L|%N|%Q|%R|%S|%T|%U|%V|%%|%\'|%\(|%\)|%<|%>|%\/{I}\/|%\/0[xX]{H}(({H}|_)*{H})?\/|%\/0[cC]{O}(({O}|_)*{O})?\/|%\/0[bB]{B}(({B}|_)*{B})?\/)*[%\"] {
			-- Eiffel String
		if not in_comments then						
			curr_token := new_string
		else
			curr_token := new_comment
		end
		update_token_list
	}

----------/** Eiffel integers **/-----------------------------------------------

{I}					|
{I}/".."				|
0[xX]{H}(({H}|_)*{H})?			|
0[cC]{O}(({O}|_)*{O})?			|
0[bB]{B}(({B}|_)*{B})? {
			-- Eiffel Integer
		if not in_comments then
			curr_token := new_number
		else
			curr_token := new_comment
		end
		update_token_list
	}


0[bBcCxX]{X}+			|
{D}{X}+ {
			-- Bad Eiffel Integer
		if not in_comments then
			curr_token := new_text
		else
			curr_token := new_comment
		end
		update_token_list
	}

---------/** Eiffel reals **/---------------------------------------------------

({D}*\.{D}+{E})|({D}+\.{D}*{E})|(({U}(_{T})*)?\.({T}_)*{U}{E})|({U}(_{T})*\.(({T}_)*{U})?{E}) {
			-- Eiffel reals & doubles
		if not in_comments then		
			curr_token := new_number
		else
			curr_token := new_comment
		end
		update_token_list
	}

		-- The first and fourth expressions use a trailing context
		-- to make sure that an integer followed by two dots is
		-- not recognized as a real followed by a dot.

----------/** Comments enable/disable **/-----------------------------------------

\`[^%\n\`']+\`					|
\`[^%\n\`']+\' {
		curr_token := new_text_in_comment
		update_token_list
	}

--------------------------------------------------------------------------------

<<EOF>>			terminate
. {
			-- Error (considered as text)
		if not in_comments then
			curr_token := new_text
		else
			curr_token := new_comment
		end
		update_token_list
	}

