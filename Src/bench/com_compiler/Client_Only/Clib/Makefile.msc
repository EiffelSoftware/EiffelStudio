all:: ecom.lib ecom_final.lib

ecom.lib: $(WOBJ)
	if exist $@ del $@
	lib -OUT:$@ $(WOBJ)
	del *.obj
	if not exist msc mkdir msc
	$(MV) $@ msc
	del $@

ecom_final.lib: $(OBJ)
	if exist $@ del $@
	lib -OUT:$@ $(OBJ)
	del *.obj
	if not exist msc mkdir msc
	$(MV) $@ msc
	del $@

.cpp.obj:
