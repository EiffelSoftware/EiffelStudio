<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="store" uuid="2f4368c4-b48f-454f-9206-f7ef7c39b23d" library_target="store">
  <target name="store">
    <description>
      EiffelStore: Relational database mapping for Eiffel.
      Copyright (c) 1984-2006, Eiffel Software and others.
      Eiffel Forum License v2 (see http://www.eiffel.com/licensing/forum.txt).
    </description>
    <root all_classes="true"/>
    <file_rule>
      <exclude>/\.svn</exclude>
    </file_rule>
    <option namespace="EiffelSoftware.Library.Store"/>
    <setting name="dotnet_naming_convention" value="true"/>
    <library name="base" location="$ISE_LIBRARY\library\base\base.ecf"/>
    <library name="vision2" location="$ISE_LIBRARY\library\vision2\vision2.ecf"/>
    <library name="time" location="$ISE_LIBRARY\library\time\time.ecf"/>
    <library name="lex" location="$ISE_LIBRARY\library\lex\lex.ecf"/>
    <cluster name="converter" location="converter" recursive="true">
      <option namespace="Converter"/>
    </cluster>
    <cluster name="dataview" location="dataview" recursive="true">
      <option namespace="DataView"/>
    </cluster>
    <cluster name="dbms-support" location="dbms/support" recursive="true">
      <option namespace="Dbms.Support"/>
    </cluster>
    <cluster name="generation" location="generation" recursive="true">
      <option namespace="Generation"/>
    </cluster>
    <cluster name="interface" location="interface" recursive="true">
      <option namespace="Interface"/>
    </cluster>
    <cluster name="mapping" location="mapping" recursive="true">
      <option namespace="Mapping"/>
    </cluster>
    <cluster name="support" location="support" recursive="true">
      <option namespace="Support"/>
    </cluster>
  </target>
</system>
