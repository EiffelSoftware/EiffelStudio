system
	"ecdpman"

root
	ECDM_APPLICATION (root_cluster): "make_and_launch"

default
	assertion (all)
	msil_generation (yes)
	il_verifiable (yes)
	cls_compliant (yes)
	line_generation (yes)
	dotnet_naming_convention (yes)
	msil_generation_type ("exe")
	msil_key_file_name ("$EIFFEL_SRC\dotnet\helpers\isekey.snk")
	use_cluster_name_as_namespace (yes)
	use_all_cluster_name_as_namespace (yes)
	precompiled ("$EIFFEL_SRC\dotnet\codedom_provider\build_studio_debug\EiffelSoftware.CodeDomVision2")
	msil_use_optimized_precompile (yes)
	version ("2.0.0.0")


cluster
	root_cluster:		"$EIFFEL_SRC\dotnet\codedom_provider\manager"

	codedom_provider:		"$EIFFEL_SRC\dotnet\codedom_provider\common"

	all eclop:		"$ECLOP\eclop"


end

