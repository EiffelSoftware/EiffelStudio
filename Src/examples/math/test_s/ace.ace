system
	test_s

root
	TEST_S (root_cluster): "make"

default
	assertion(all);
	precompiled ("$ISE_LIBRARY/precomp/spec/$ISE_PLATFORM/base")

cluster
	root_cluster:		".";

	bvp:		"$ISE_LIBRARY\library\math\bvp";
	eifmath:	"$ISE_LIBRARY\library\math\eifmath";
	integrat:	"$ISE_LIBRARY\library\math\integrat";
	interpol:	"$ISE_LIBRARY\library\math\interpol";
	linear:		"$ISE_LIBRARY\library\math\linear";
	ode:		"$ISE_LIBRARY\library\math\ode";
	optima: 	"$ISE_LIBRARY\library\math\optima";
	random: 	"$ISE_LIBRARY\library\math\random";
	regress:	"$ISE_LIBRARY\library\math\regress";
	roots:		"$ISE_LIBRARY\library\math\roots";
	special:	"$ISE_LIBRARY\library\math\special";
	transp:		"$ISE_LIBRARY\library\math\transp";
	tsa:		"$ISE_LIBRARY\library\math\tsa";

external
	Include_path: "$(NAGCL04_32)\include", "$(ISE_LIBRARY)\library\math\clib";
	Object: "$(ISE_LIBRARY)\library\math\clib\eifmathc.lib", "$(NAGCL04_32)\dll\nagc.lib";

end

