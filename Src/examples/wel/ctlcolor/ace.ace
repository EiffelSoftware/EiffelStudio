system
	ctlcolor_demo

root 
	CTLCOLOR_DEMO (root_cluster): "make"

default
	assertion (all)

cluster 
	root_cluster:	"$EIFFEL4\examples\wel\ctlcolor"

		-- WEL
	all wel: 		"$EIFFEL4\library\wel"

		-- EiffelBase
	all base: 		"$EIFFEL4\library\base"
		exclude
			"table_eiffel3"
			"desc"
		end

external
	include_path:	"$EIFFEL4\library\wel\spec\windows\include"
	object: 		"$EIFFEL4\library\wel\spec\msc\lib\wel.lib"

end -- ctlcolor_demo
