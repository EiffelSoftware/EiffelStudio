-- This is a test control file

test_name inline-agent-of-descendant
test_description An inline agent created in a descendant class is callable

copy_sub Ace $TEST Ace
copy_raw a.e $CLUSTER a.e
copy_raw b.e $CLUSTER b.e

compile_frozen
compile_result ok

c_compile_work
c_compile_result	ok

execute_work 	NONE 	exec_output
execute_result 	ok
compare		exec_output output

test_end
