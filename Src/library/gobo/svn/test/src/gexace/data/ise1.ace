system

	xace1

root

	FOO: "make"

default

	address_expression (no)
	array_optimization (no)
	assertion (require)
	check_vape (yes)
	cls_compliant (yes)
	console_application (yes)
	dead_code_removal (yes)
	debug (no)
	dotnet_naming_convention (no)
	dynamic_runtime (no)
	exception_trace (no)
	il_verifiable (yes)
	inlining (yes)
	inlining_size ("0")
	line_generation (no)
	msil_generation (no)
	multithreaded (no)
	profile (no)
	msil_generation_type ("exe")
	trace (no)
	use_cluster_name_as_namespace (yes)
	use_all_cluster_name_as_namespace (yes)

cluster

	all gefoo: "${GOBO}/test/src/gexace/data"
	library base: "${ISE_LIBRARY}/library/base"
		exclude
			".svn";
			"table_eiffel3"
		end

end
