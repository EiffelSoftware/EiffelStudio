system
	test

root
	EV_TEST: start

default

	assertion (require)
	--debug("EVENT_TRACE")
	--debug("EV_GTK_DEBUG")
cluster

dev:						"$VISION_PRERELEASE"

gel(dev):					"$/gel"

all vision2(dev):				"$/vision2"
	exclude
		"mswin";
		"EIFGEN";
		"obsolete";
	end

all base(dev):					"$/base"
	exclude
		"table_eiffel3"
	end

external

	include_path:
	"$VISION_PRERELEASE/vision2/spec/include/",
	"$VISION_PRERELEASE/vision2/implementation/gtk/Clib `gtk-config --cflags`";

	object:
	"$(VISION_PRERELEASE)/vision2/Clib/load_pixmap.o",
	"$(VISION_PRERELEASE)/vision2/implementation/gtk/Clib/gtk_eiffel.o",
	"`gtk-config --libs` -lpng -lz -lXtst"

end
