<?xml version="1.0" encoding="ISO-8859-1"?><system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="editor" uuid="7521917C-F584-4F7F-AF8B-DF0F91BFA290" library_target="editor">
	<target name="editor">
		<description>Editor library</description>
		<file_rule>
			<exclude>/\.svn</exclude>
			<exclude>/eiffel$</exclude>
			<exclude>/text_formatter\.e$</exclude>
		</file_rule>
		<option>
			<assertions precondition="true" postcondition="false" check="false" invariant="false" loop="false"/>
		</option>
		<library name="base" location="$ISE_LIBRARY\library\base\base.ecf" readonly="true"/>
		<library name="gobo" location="$ISE_LIBRARY\library\gobo.ecf" readonly="true"/>
		<library name="lex" location="$ISE_LIBRARY\library\lex\lex.ecf" readonly="true"/>
		<library name="preferences_reg" location="$ISE_LIBRARY\library\preferences\preferences_reg.ecf" readonly="true">
			<condition><platform value="windows"/></condition>
		</library>
		<library name="preferences_xml" location="$ISE_LIBRARY\library\preferences\preferences_xml.ecf" readonly="true">
			<condition><platform excluded_value="windows"/></condition>
		</library>
		<library name="vision2" location="$ISE_LIBRARY\library\vision2\vision2.ecf" readonly="true"/>
		<cluster name="document" location="document\" recursive="true"/>
		<cluster name="editor" location=".\"/>
		<cluster name="managers" location="managers\" recursive="true"/>
		<cluster name="text_window" location="text_window\" recursive="true">
			<file_rule>
				<exclude>^/text/lexer/syntax_definitions</exclude>
			</file_rule>
		</cluster>
	</target>
</system>
