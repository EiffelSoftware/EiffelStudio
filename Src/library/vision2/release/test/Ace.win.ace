system
	test

root
	EV_TEST: make_and_launch

default
	assertion (all)

cluster
	library:				"$EIFFEL4\library"

	all wel (library):			"$\wel"

	all vision2 (library):			"$\vision2"
		exclude
			"gtk"
			"obsolete"
		end

	all base (library):			"$\base"
		exclude
			"table_eiffel3"
		end

external
        include_path:
			"$EIFFEL4\library\wel\spec\windows\include",
			"$EIFFEL4\library\vision2\spec\include"
        object:
			"$EIFFEL4\library\wel\spec\$(COMPILER)\lib\wel.lib",
			"$EIFFEL4\library\vision2\spec\$(COMPILER)\lib\libpng.lib",
			"$EIFFEL4\library\vision2\spec\$(COMPILER)\lib\zlib.lib",
			"$EIFFEL4\library\vision2\spec\$(COMPILER)\lib\vision2.lib"
end
