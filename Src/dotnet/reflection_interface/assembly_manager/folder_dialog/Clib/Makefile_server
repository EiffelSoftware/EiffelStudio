# ecom.lib - Makefile for Microsoft C

CC = cl
CFLAGS = /W0 /Ox /DWORKBENCH /D "_WIN32_DCOM" /c /Ic:\eiffel50\bench\spec\windows\include /Ic:\50dev\library\com\spec\windows\include 

OBJ = ecom_folderbrowser_folderbrowser_s.obj ecom_folderbrowser_ifolderbrowser_impl_stub_s.obj ecom_gce_folder_browser.obj ecom_gec_folder_browser.obj folderbrowser_factory.obj server_registration.obj 

ecom_server.lib: $(OBJ)
	if exist $@ del $@
	lib /OUT:$@ $(OBJ)
	del *.obj

.cpp.obj:
	$(CC) $(CFLAGS) $<
