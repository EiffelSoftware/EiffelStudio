-- This is a test control file

test_name 	invalid-assertion-level
test_description After initial compilation where the system is frozen, an attribute is changed from a BIT_REF to a BOOL_STRING and the instruction which creates it is modified accordingly and changes are melted

copy_sub 	Ace $TEST Ace
define		ATTRIBUTE_TYPE "BIT_REF"
define		CREATION_INST "bitmap := new_bitmap"
copy_sub 	test.e $CLUSTER test.e
compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work 	NONE 	exec_output1
execute_result 	ok

define		ATTRIBUTE_TYPE "BOOL_STRING"
define		CREATION_INST "!!bitmap.make (Bitmap_length)"
copy_sub 	test.e $CLUSTER test.e
compile_melted
compile_result 	ok

execute_work 	NONE 	exec_output2
execute_result 	ok

test_end
