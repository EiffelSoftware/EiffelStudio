#/bin/sh!
# script to set up the directory minimal_support

cd $1
echo Setup minimal_support in $1

if [ ! -d minimal_support ]
then
    mkdir minimal_support
    cd minimal_support
    ln -s $EIFFEL4/library/base/structures/access/active.e ./active.e
    ln -s $EIFFEL4/library/base/structures/list/arrayed_list.e ./arrayed_list.e
    ln -s $EIFFEL4/library/base/structures/cursors/arrayed_list_cursor.e ./arrayed_list_cursor.e
    ln -s $EIFFEL4/library/base/structures/access/bag.e ./bag.e
    ln -s $EIFFEL4/library/base/structures/traversing/bilinear.e ./bilinear.e
    ln -s $EIFFEL4/library/base/structures/storage/bounded.e ./bounded.e
    ln -s $EIFFEL4/library/base/structures/storage/box.e ./box.e
    ln -s $EIFFEL4/library/base/structures/list/chain.e ./chain.e
    ln -s $EIFFEL4/library/base/structures/access/collection.e ./collection.e
    ln -s $EIFFEL4/library/base/structures/access/container.e ./container.e
    ln -s $EIFFEL4/library/base/structures/storage/countable.e ./countable.e
    ln -s $EIFFEL4/library/base/structures/cursors/cursor.e ./cursor.e
    ln -s $EIFFEL4/library/base/structures/access/cursor_structure.e ./cursor_structure.e
    ln -s $EIFFEL4/library/base/structures/list/dynamic_chain.e ./dynamic_chain.e
    ln -s $EIFFEL4/library/base/structures/list/dynamic_list.e ./dynamic_list.e
    ln -s $EIFFEL4/library/base/structures/storage/finite.e ./finite.e
    ln -s $EIFFEL4/library/base/structures/access/indexable.e ./indexable.e
    ln -s $EIFFEL4/library/base/structures/storage/infinite.e ./infinite.e
    ln -s $EIFFEL4/library/base/structures/traversing/linear.e ./linear.e
    ln -s $EIFFEL4/library/base/structures/list/list.e ./list.e
    ln -s $EIFFEL4/library/base/structures/storage/resizable.e ./resizable.e
    ln -s $EIFFEL4/library/base/structures/list/sequence.e ./sequence.e
    ln -s $EIFFEL4/library/base/structures/access/table.e ./table.e
    ln -s $EIFFEL4/library/base/structures/traversing/traversable.e ./traversable.e
    ln -s $EIFFEL4/library/base/structures/storage/unbounded.e ./unbounded.e
else
    echo "minimal_support already exists"
fi
