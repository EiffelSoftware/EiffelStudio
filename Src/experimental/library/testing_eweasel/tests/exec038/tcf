-- This is a test control file

test_name 	 once-expanded-result
test_description A system which calls a once function whose result type is an expanded type

copy_sub 	 Ace $TEST Ace
define		 CREATION "!!Result"
copy_sub 	 test.e  $CLUSTER test.e
copy_raw 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 ok

execute_work	 NONE exec_output1
execute_result	 ok
compare		 exec_output1 output1

compile_frozen
compile_result 	 ok

c_compile_work
c_compile_result ok

execute_work	 NONE exec_output2
execute_result	 ok
compare		 exec_output2 output1

define		 CREATION ""
copy_sub 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

define		 CREATION "!!Result"
copy_sub 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

execute_work	 NONE exec_output3
execute_result	 ok
compare		 exec_output3 output1

test_end
