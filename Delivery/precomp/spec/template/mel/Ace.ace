system MEL

root

	ANY

default

	assertion (require);
	precompiled ("$EIFFEL5/precomp/spec/$PLATFORM/base")

cluster

-- MEL
	all mel:				"$EIFFEL5/library/mel/"

external
 
	include_path:
		"\$(EIFFEL5)/library/mel/Clib";
 
	object:
		"$(EIFFEL5)/library/mel/spec/$(PLATFORM)/lib/libMel.a",
		"$(EIFFEL5)/library/mel/spec/$(PLATFORM)/lib/libXpm.a",
		"-lXm -lXt -lX11"
 
end
