#!/bin/sh

remtrace Retrieve libraries
cd $INSTALL_DIR
md library
cd library
remtrace base
exprt $DEFAULT_CVS_TAG/Src/library/base base 
remtrace diff
exprt $DEFAULT_CVS_TAG/Src/library/diff diff
remtrace Eiffel2Java
exprt $DEFAULT_CVS_TAG/Src/library/Eiffel2Java Eiffel2Java 
remtrace event
exprt $DEFAULT_CVS_TAG/Src/library/event event 
remtrace graph
exprt $DEFAULT_CVS_TAG/Src/library/graph graph 
remtrace lex
exprt $DEFAULT_CVS_TAG/Src/library/lex lex 
remtrace mel
exprt $DEFAULT_CVS_TAG/Src/library/mel mel 
remtrace memory_analyzer
exprt $DEFAULT_CVS_TAG/Src/library/memory_analyzer memory_analyzer 
remtrace net
exprt $DEFAULT_CVS_TAG/Src/library/net net 
remtrace obsolete
exprt $DEFAULT_CVS_TAG/Src/library/obsolete obsolete 
remtrace parse
exprt $DEFAULT_CVS_TAG/Src/library/parse parse 
remtrace preferences
exprt $DEFAULT_CVS_TAG/Src/library/preferences preferences 
remtrace process
exprt $DEFAULT_CVS_TAG/Src/library/process process 
remtrace store
exprt $DEFAULT_CVS_TAG/Src/library/store store 
remtrace thread
exprt $DEFAULT_CVS_TAG/Src/library/thread thread 
remtrace time
exprt $DEFAULT_CVS_TAG/Src/library/time time 
remtrace uuid
exprt $DEFAULT_CVS_TAG/Src/library/uuid uuid 
remtrace vision
exprt $DEFAULT_CVS_TAG/Src/library/vision vision 
remtrace vision2
exprt $DEFAULT_CVS_TAG/Src/library/vision2 vision2 
fullrd vision2/implementation/mswin
if [ "$1" = "gtk" ]; then
	# TODO
	# Copy GTK 1.2 implementation over gtk directory
	echo ""
fi
remtrace web
exprt $DEFAULT_CVS_TAG/Src/library/web web 

cd ../..

remtrace Retrieve examples
cd $INSTALL_DIR
md examples
cd examples
remtrace base
exprt $DEFAULT_CVS_TAG/Src/examples/base base 
remtrace Eiffel2Java
exprt $DEFAULT_CVS_TAG/Src/examples/Eiffel2Java Eiffel2Java 
remtrace cecil
exprt $DEFAULT_CVS_TAG/Src/examples/cecil cecil 
remtrace cpp
exprt $DEFAULT_CVS_TAG/Src/examples/cpp cpp 
remtrace lex
exprt $DEFAULT_CVS_TAG/Src/examples/lex lex 
remtrace net
exprt $DEFAULT_CVS_TAG/Src/examples/net net 
remtrace parse
exprt $DEFAULT_CVS_TAG/Src/examples/parse parse 
remtrace preferences
exprt $DEFAULT_CVS_TAG/Src/examples/preferences preferences 
remtrace store
exprt $DEFAULT_CVS_TAG/Src/examples/store store 
fullrd store/matisse
remtrace thread
exprt $DEFAULT_CVS_TAG/Src/examples/thread thread 
cd thread
fullrd fancy
cd ..
remtrace time
exprt $DEFAULT_CVS_TAG/Src/examples/time time 
remtrace vision
exprt $DEFAULT_CVS_TAG/Src/examples/vision vision 
remtrace vision2
exprt $DEFAULT_CVS_TAG/Src/examples/vision2 vision2 
cd vision2
fullrd Boxes
fullrd drawing_area
fullrd ev_list
fullrd fake_event
fullrd figure
fullrd grid
fullrd hello_world
fullrd menu
fullrd multicolumn_list
fullrd pixmap
fullrd split_area
fullrd test_all_widgets
fullrd test_events
fullrd widget_test
fullrd tests
fullrd tutorial
cd ..
remtrace web
exprt $DEFAULT_CVS_TAG/Src/examples/web web 
remtrace studio
exprt $DEFAULT_CVS_TAG/Src/examples/studio studio 

cd ../..
