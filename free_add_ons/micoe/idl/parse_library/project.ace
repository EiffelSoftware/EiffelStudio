system

    project_compiler

root

    PROJECT_COMPILER (start) : "make"

default

    assertion (all)

cluster

    start:   ".";
    lex:     "$EIFFEL_S/lex_library";
    parse:   "$EIFFEL_S/parse_library";
    kernel:  "$EIFFEL4/library/base/kernel";
    minimal: "$EIFFEL4/library/minimal_support";
    contain: "$EIFFEL_S/library/contain";
    basic:   "$EIFFEL_S/library/basic";

external
    Object: "$(EIFFEL_S)/lib/basic.a"

end
