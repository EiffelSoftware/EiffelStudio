indexing
	description: "Objects that represent an EV_DIALOG.%
		%The original version of this class was generated by EiffelBuild."
	date: "$Date$"
	revision: "$Revision$"

class
	GB_IMPORT_DIALOG

inherit
	GB_IMPORT_DIALOG_IMP


feature {NONE} -- Initialization

	user_initialization is
			-- called by `initialize'.
			-- Any custom user initialization that
			-- could not be performed in `initialize',
			-- (due to regeneration of implementation class)
			-- can be added here.
		do
		--	change_list.disable_edit
			change_list.set_background_color ((create {EV_STOCK_COLORS}).white)
			set_default_push_button (ok_button)
		end
		
feature -- Basic operation

	add_output (an_output: STRING) is
			-- Add text representation of `an_output' to `change_list'.
		do
			change_list.append_text (an_output + "%N")		
		end
		

feature {NONE} -- Implementation

	
	ok_pressed is
			-- Called by `select_actions' of `ok_button'.
		do
			hide
			destroy
		end

end -- class GB_IMPORT_DIALOG

