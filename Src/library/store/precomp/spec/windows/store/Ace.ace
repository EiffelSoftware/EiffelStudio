system EiffelStore_EiffelBase

root

	ANY

default

	assertion (require)

cluster

    -- EiffelBase
	access:             "$ISE_EIFFEL/library/base/structures/access";
	cursors:            "$ISE_EIFFEL/library/base/structures/cursors";
	cursor_tree:        "$ISE_EIFFEL/library/base/structures/cursor_tree";
	dispenser:          "$ISE_EIFFEL/library/base/structures/dispenser";
	iteration:          "$ISE_EIFFEL/library/base/structures/iteration";
	kernel:             "$ISE_EIFFEL/library/base/kernel";
	list:               "$ISE_EIFFEL/library/base/structures/list";
	set:                "$ISE_EIFFEL/library/base/structures/set";
	sort:               "$ISE_EIFFEL/library/base/structures/sort";
	storage:            "$ISE_EIFFEL/library/base/structures/storage";
	support:            "$ISE_EIFFEL/library/base/support";
	table:              "$ISE_EIFFEL/library/base/structures/table";
	traversing:         "$ISE_EIFFEL/library/base/structures/traversing";
	tree:               "$ISE_EIFFEL/library/base/structures/tree";
 
	-- EiffelStore

	estore_support:		"$ISE_EIFFEL/library/store/support";
	estore_interface:	"$ISE_EIFFEL/library/store/interface";
	estore_db_support:	"$ISE_EIFFEL/library/store/dbms/support";

	-- EiffelTime

	time:				"$ISE_EIFFEL/library/time";
	time_format: 		"$ISE_EIFFEL/library/time/format";
	time_english:		"$ISE_EIFFEL/library/time/format/english";

external

	object:
		"$(ISE_EIFFEL)/library/store/spec/$(ISE_C_COMPILER)/lib/support.lib",
		"$(ISE_EIFFEL)/library/time/spec/$(ISE_C_COMPILER)/lib/datetime.lib";

end
