all:: ecom.lib ecom_final.lib

ecom.lib: $(WOBJ)
	if exist $@ del $@
	lib -OUT:$@ $(WOBJ)
	if not exist msc mkdir msc
	$(MV) $@ msc
	del $@

ecom_final.lib: $(OBJ)
	if exist $@ del $@
	lib -OUT:$@ $(OBJ)
	if not exist msc mkdir msc
	$(MV) $@ msc
	del $@

