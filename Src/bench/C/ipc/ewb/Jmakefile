/*
 * Jmakefile for ewb (client)
 */

;# Files used to build the daemon
SRC = ewb_proto.c eproto.c eif_in.c eif_out.c ewb_init.c ewb_dumped.c

;# Derived object file names
OBJ = \
|expand f!$(SRC) $(LSRC)!
	!f:\.[cl]=.o \
-expand \\

;# Where shared archive is located (path and name)
LIBDIR = ../shared
LIBIDR = ../../idrs

/* The following are understood by jmake to alter built-in commands */
RUN_TIME = ../../run-time
DPFLAGS = -I$(TOP) -I$(LIBDIR) -I$(RUN_TIME) -I$(LIBIDR)
CFLAGS = $(DPFLAGS)

DependTarget()
NormalLibraryTarget(ewb,$(SRC),$(OBJ))

