cluster
	all root_cluster:		"$EIFFEL_SRC/examples/vision2/widget_test"
		exclude
			"templates"; 
		end

	all base:		"$ISE_EIFFEL/library/base"
		exclude
			"table_eiffel3"; "desc"; 
		end

	all contrib: "$EIFFEL_SRC/library/vision2/contrib"
		exclude
			"mswin"
		end

	all vision2:		"$EIFFEL_SRC/library/vision2"
		exclude
			"contrib";"mswin"; "ev_pixmap.e"; "ev_pixmapable.e"; "gtk2"; 
		end

external
	include_path:
			"\$(ISE_EIFFEL)/library/event/Clib",
			"\$(ISE_EIFFEL)/library/vision2/spec/include",
			"\$(ISE_EIFFEL)/library/vision2/implementation/gtk/Clib `$ISE_EIFFEL/library/vision2/implementation/gtk/Clib/vision2-gtk-config --include_path`"
	object:
			"$(ISE_EIFFEL)/library/vision2/spec/$(ISE_PLATFORM)/lib/gtk_eiffel.o `$ISE_EIFFEL/library/vision2/implementation/gtk/Clib/vision2-gtk-config --object`"
end
