system
	our_client

root
	OUR_CLIENT: make_client

default
	assertion(no)
	arguments("localhost 6000")
	arguments(" ")
	disabled_debug(yes)
	debug(no)
	line_generation(no)
	profile(no)
	trace(no)
	il_verifiable(no)
	msil_generation_type("exe")
	check_vape(yes)
	console_application(no)
	address_expression(no)
	array_optimization(no)
	dead_code_removal(yes)
	dynamic_runtime(no)
	exception_trace(no)
	inlining(no)
	multithreaded(no)
	precompiled ("$ISE_LIBRARY\precomp\spec\$ISE_PLATFORM\base")

cluster
	root_cluster:		"$ISE_LIBRARY\examples\net\predef\client"


	common_classes:		"$ISE_LIBRARY\examples\net\predef\common"

	net:		"$ISE_LIBRARY\library\net"

	abstract_socket (net):		"$\abstract\socket"

	net_socket (net):		"$\network\socket"

	local_socket (net):		"$\local\socket"

	polling_abstract (net):		"$\polling\abstract"

	polling_implementation (net):		"$\polling\implementation"

	abstract_client_server (net):		"$\abstract\client_server"

	net_client_server (net):		"$\network\client_server"

	local_client_server (net):		"$\local\client_server"

	all base:		"$ISE_LIBRARY\library\base"
		exclude
			"desc"; "table_eiffel3"; 
		end

external

	object:
		"$(ISE_LIBRARY)\library\net\spec\$(ISE_C_COMPILER)\lib\net.lib"

end
