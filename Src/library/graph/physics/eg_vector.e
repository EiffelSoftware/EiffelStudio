deferred class
	EG_VECTOR [G -> NUMERIC]

inherit
	NUMERIC
		rename
			product as product alias "*"
		end

feature -- Basic operations

	scalar_product alias "|*" (other: G): like Current
			-- Scalar product between `Current' and other.
		deferred
		end

