system
	"EiffelSoftware.CodedomSerializer"

root
	NONE

default
	precompiled ("$EIFFEL_SRC\dotnet\codedom_provider_2.0\build_studio_debug\EiffelSoftware.CodeDom.Vision2")
	msil_use_optimized_precompile (yes)
	assertion (no)
	msil_generation (yes)
	il_verifiable (yes)
	cls_compliant (yes)
	dotnet_naming_convention (yes)
	use_cluster_name_as_namespace (yes)
	use_all_cluster_name_as_namespace (yes)
	msil_generation_type ("dll")
	version ("3.0.0.0")
	msil_key_file_name ("$EIFFEL_SRC\dotnet\helpers\isekey.snk")
	msil_clr_version ("v2.0.50727")
	check_vape (yes)
	console_application (no)

cluster
	all serializer:		"$EIFFEL_SRC\dotnet\codedom_provider_2.0\test\serializer"
		exclude
			"CVS"; "gui"; 
		end

	all "helpers":		"$EIFFEL_SRC\dotnet\helpers"
		exclude
			"CVS"; "classic"; "assembly_resolver";
		end

end
