#!/bin/sh

remtrace Retrieve all source files
remtrace The wizards
cd $EIFFEL_SRC
safe_md help
cd help
exprt -r $DEFAULT_CVS_TAG -d wizards "Src/help/wizards"

cd $EIFFEL_SRC/help/wizards
DELIVERY=$INSTALL_DIR
remtrace Launch the actual wizard compilation
cp compile_wizards $EXPORT_DIR
source premake_wizards 

