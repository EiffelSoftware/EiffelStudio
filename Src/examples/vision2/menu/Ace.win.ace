--! date: "$Date$
--!----------------------------------------------------------------

system
	fig -- Your executable.

root
	FIGURE_TEST (root_cluster): "make_and_launch"

default
	assertion (all)

cluster

	root_cluster:			"."
	devel:					"c:\work"
	all base (devel):		"$\base"
		exclude
			"table_eiffel3"
		end

	all wel (devel):		"$\wel"
	event (devel):			"$\event"

	all vision2 (devel):	"$\vision2"
		exclude 
			"gtk"
			"EIFGEN"
		end

external
        include_path:
			"c:\work\wel\spec\windows\include"
        object:
			"c:\work\wel\spec\$(COMPILER)\lib\wel.lib"

end

