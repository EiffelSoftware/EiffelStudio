CC = cl
CFLAGS = -MT -DEIF_THREADS -I. -I$(EIFFEL4)\bench\spec\windows\include 
OBJECTS = main.obj fext.obj
EIFLIB = libbank_account.lib
LD = link
MAKE=nmake

all: $(OBJECTS) $(EIFLIB) cecil

main.obj: 
	$(CC) $(CFLAGS) -c main.c -Fo$@

fext.obj: 
	$(CC) $(CFLAGS) -c fext.c -Fo$@ 

$(EIFLIB):
	cd EIFGEN\F_code
	$(MAKE) cecil
	cd ..\..
cecil: $(OBJECTS)  $(EIFLIB)
	$(LD) @cecil.lnk




