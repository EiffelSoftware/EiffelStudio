# exml.lib - Makefile for Microsoft C

CC = cl
CFLAGS = -c -Ox -I$(EXML)/lib/implementation/expat/compiler_specific/ise/clib -I$(EXPAT)/xmlparse -I$(ISE_EIFFEL)/studio/spec/windows/include
.c.obj:
	$(CC) $(CFLAGS) $<

OBJ = exml_parser.obj

exml.lib: $(OBJ)
	if exist $@ del $@
	lib /OUT:$@ $(OBJ)
	if not exist $(EXML)/lib/implementation/expat/compiler_specific/ise/clib/msc 
	mkdir $(EXML)/lib/implementation/compiler_specific/ise/clib/msc
	copy $@ $(EXML)/lib/implementation/expat/compiler_specific/ise/clib/msc
	del $@
	del *.obj
