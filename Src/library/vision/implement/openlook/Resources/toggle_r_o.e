
-- Toggle button defines

indexing

	status: "See notice at end of class";
        date: "$Date$";
        revision: "$Revision$"

class  TOGGLE_R_O



        
feature {NONE}

        Oarm: STRING is "select";
                        -- Name of openlook resource

        Odisarm: STRING is "unselect";
                        -- Name of openlook resource

        Oset: STRING is "set";
                        -- Name of openlook resource

end 


