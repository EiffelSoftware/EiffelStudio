# Requires environment variable ISE_EIFFEL.


CC = cl
CFLAGS = -c -Ox -MT -D "_WIN32_DCOM" -I$(ISE_EIFFEL)\studio\spec\$(ISE_PLATFORM)\include\ -I..\spec\windows\include -c -Tp

OBJ = E_variant.obj E_IRootStorage.obj E_IStorage.obj E_IStream.obj E_wide_string.obj E_Routines.obj ecom_exception.obj E_arraydesc.obj E_custdata.obj E_bstr.obj E_Currency.obj E_Decimal.obj E_generic_interface.obj E_automation_interface.obj ecom_font_impl_proxy.obj ecom_ienumvariant_impl_proxy.obj ecom_ifont_impl_proxy.obj ecom_picture_impl_proxy.obj 

com.lib: $(OBJ)
	if exist $@ del $@
	lib /OUT:$@ $(OBJ)
	if not exist ..\spec\msc mkdir ..\spec\msc
	if not exist ..\spec\msc\lib mkdir ..\spec\msc\lib
	copy $@ ..\spec\msc\lib
	del $@
	del *.obj

.cpp.obj:
	$(CC) $(CFLAGS) $<

.c.obj:
	$(CC) $(CFLAGS) $<
