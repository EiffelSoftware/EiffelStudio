<EIFFEL_DOCUMENT>
<TOPIC TOPIC_ID="EiffelBuild_preferences" ICON="icon_preferences_root">
	<HEAD>EiffelBuild Resources</HEAD>
	<TOPIC TOPIC_ID="Global_preferences" ICON="icon_preferences_folder">
		<HEAD>Global preferences</HEAD>
		<TEXT DESCRIPTION="Text files format: True = Windows / False = UNIX">text_mode_is_windows<BOOLEAN>True</BOOLEAN></TEXT>
	</TOPIC>
	<TOPIC TOPIC_ID="Tools" ICON="icon_preferences_folder">
		<HEAD>Tools</HEAD>
		<TOPIC TOPIC_ID="Browsing_tools" ICON="icon_preferences_folder">
			<HEAD>Browsing tools</HEAD>
			<TEXT DESCRIPTION="Maximum number of items displayed in the history combo boxes">history_size<INTEGER>10</INTEGER></TEXT>
			
		</TOPIC>
		<TOPIC TOPIC_ID="Context_tool" ICON="icon_preferences_folder">
			<HEAD>Context tools</HEAD>
			<TEXT DESCRIPTION="Link the context tool with other components by default?">unified_stone<BOOLEAN>False</BOOLEAN></TEXT>
			
		</TOPIC>
		<TOPIC TOPIC_ID="Editor" ICON="icon_preferences_folder">
			<HEAD>Editor</HEAD>
			<TEXT DESCRIPTION="Automatic update when the file has not been modified">automatic_update<BOOLEAN>True</BOOLEAN></TEXT>
			
			<TOPIC TOPIC_ID="Keyboard_shortcuts" ICON="icon_preferences_folder">
				<HEAD>Keyboard Shortcuts</HEAD>
				
				<TOPIC TOPIC_ID="Insert user defined string (3)" ICON="icon_preferences_folder">
					<HEAD>Insert user defined string (3)</HEAD>
					<TEXT DESCRIPTION="String to be inserted" IMMEDIATE_EFFECT="no">customized_string_3<STRING></STRING></TEXT>
				</TOPIC>
			</TOPIC>
			<TOPIC TOPIC_ID="Automatic completion" ICON="icon_preferences_folder">
				<HEAD>Autocomplete</HEAD>
				<TEXT DESCRIPTION="Brackets and parentheses are closed automatically">autocomplete_brackets_and_parenthesis<BOOLEAN>False</BOOLEAN></TEXT>
				<TOPIC TOPIC_ID="class_structure_completion" ICON="icon_preferences_folder">
					<HEAD>Class structure keywords</HEAD>
					<TOPIC TOPIC_ID="feature" ICON="icon_preferences_folder">
						<HEAD>feature</HEAD>
						<TEXT DESCRIPTION="Autocomplete feature">autocomplete_feature<BOOLEAN>True</BOOLEAN></TEXT>
	
					</TOPIC>
				</TOPIC>
				<TOPIC TOPIC_ID="feature_structure_completion" ICON="icon_preferences_folder">
					<HEAD>Feature structure keywords</HEAD>
					<TOPIC TOPIC_ID="is" ICON="icon_preferences_folder">
						<HEAD>is</HEAD>
						<TEXT DESCRIPTION="Autocomplete is">autocomplete_is<BOOLEAN>True</BOOLEAN></TEXT>

					</TOPIC>
				</TOPIC>
				<TOPIC TOPIC_ID="control_structures_completion" ICON="icon_preferences_folder">
					<HEAD>Control structures keywords</HEAD>
					<TOPIC TOPIC_ID="check" ICON="icon_preferences_folder">
						<HEAD>check</HEAD>
						<TEXT DESCRIPTION="Autocomplete check">autocomplete_check<BOOLEAN>True</BOOLEAN></TEXT>
					</TOPIC>
				</TOPIC>
				<TOPIC TOPIC_ID="inherit_clause_completion" ICON="icon_preferences_folder">
					<HEAD>Inherit clauses keywords</HEAD>
					<TOPIC TOPIC_ID="rename" ICON="icon_preferences_folder">
						<HEAD>rename</HEAD>
						<TEXT DESCRIPTION="Autocomplete rename">autocomplete_rename<BOOLEAN>True</BOOLEAN></TEXT>
					</TOPIC>
				</TOPIC>
				<TOPIC TOPIC_ID="misc_completion" ICON="icon_preferences_folder">
					<HEAD>Other keywords</HEAD>
					<TOPIC TOPIC_ID="end" ICON="icon_preferences_folder">
						<HEAD>end</HEAD>
						<TEXT DESCRIPTION="Autocomplete end">autocomplete_end<BOOLEAN>False</BOOLEAN></TEXT>

					</TOPIC>
				</TOPIC>
			</TOPIC>
		</TOPIC>
		<TOPIC TOPIC_ID="Internal_data" ICON="icon_preferences_folder" VISIBILITY="hidden">
			<HEAD>Internal Data</HEAD>
			<TEXT>graphical_output_disabled<BOOLEAN>False</BOOLEAN></TEXT>
		</TOPIC>
	</TOPIC>
	<TOPIC TOPIC_ID="Graphics" ICON="icon_preferences_folder">
		<HEAD>Graphical Elements</HEAD>
		<TEXT DESCRIPTION="All-purpose editor font" IMMEDIATE_EFFECT="no">editor_font<FONT>Courier New-r-regular-11-typewriter</FONT></TEXT>
		<TEXT DESCRIPTION="Keyword editor font" IMMEDIATE_EFFECT="no">keyword_font<FONT>Courier New-r-bold-11-typewriter</FONT></TEXT>
		<TEXT DESCRIPTION="Tabulation size" IMMEDIATE_EFFECT="no">tab_step<INTEGER>4</INTEGER></TEXT>
		<TEXT DESCRIPTION="Use animated icons">use_animated_icons<BOOLEAN>True</BOOLEAN></TEXT>
		<TOPIC TOPIC_ID="Default_Colors" ICON="icon_preferences_folder">
			<HEAD>Colors</HEAD>
			<TEXT DESCRIPTION="Color used to display normal text">normal_text_color<COLOR>0;0;0</COLOR></TEXT>
		</TOPIC>
		<TOPIC TOPIC_ID="Dialogs" ICON="icon_preferences_folder">
			<HEAD>Dialogs</HEAD>
			<TEXT DESCRIPTION="Display New/Open project dialog at startup">show_starting_dialog<BOOLEAN>True</BOOLEAN></TEXT>
		</TOPIC>
	</TOPIC>
	<TOPIC TOPIC_ID="Dynamic_data" ICON="icon_preferences_folder" VISIBILITY="hidden">
			<HEAD>Dynamic data</HEAD>
			<TEXT>build_window__height<INTEGER>640</INTEGER></TEXT>
			<TEXT>build_window__width<INTEGER>480</INTEGER></TEXT>
			<TEXT>build_window__x_position<INTEGER>200</INTEGER></TEXT>
			<TEXT>build_window__y_position<INTEGER>200</INTEGER></TEXT>
			<TEXT>main_split__position<INTEGER>100</INTEGER></TEXT>
			<TEXT>tool_order<LIST_STRING>typeselector_normal_200_200;componentselector_normal_200_200;windowselector_normal_200_200</LIST_STRING></TEXT>
			<TEXT>external_tool_order<LIST_STRING></LIST_STRING></TEXT>
			
	</TOPIC>
</TOPIC>
</EIFFEL_DOCUMENT>
