<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-4-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-4-0 http://www.eiffel.com/developers/xml/configuration-1-4-0.xsd" name="uuid-tests" uuid="6B3E4380-62CB-4137-82D1-7637F9474F79" library_target="uuid-tests">
	<description>Unit-tests for the UUID library.</description>
	<target name="uuid-tests">
		<root class="ANY" feature="default_create"/>
		<file_rule>
			<exclude>/\.svn</exclude>
			<exclude>/EIFGENs</exclude>
		</file_rule>
		<option syntax_level="1" namespace="EiffelSoftware.Library">
		</option>
		<setting name="dotnet_naming_convention" value="true"/>
		<library name="base" location="$ISE_LIBRARY\library\base\base.ecf"/>
		<library name="testing" location="$ISE_LIBRARY\library\testing\testing.ecf" readonly="false"/>
		<library name="uuid" location="$ISE_LIBRARY\library\uuid\uuid.ecf"/>
		<tests name="tests" location=".\" recursive="true">
			<option namespace="Tests">
			</option>
		</tests>
	</target>
</system>
