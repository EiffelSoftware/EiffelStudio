	mel_resources (mel):          "$/resources";
	mel_widgets (mel):            "$/widgets";


-- Eiffelvision
        vision:                         "$EIFFEL4/library/vision";
        vision_figures (vision):        "$/figures";
        vision_imp_motif (vision):      "$/implement/motif";
	vision_imp_toolkit (vision):	"$/implement/toolkit";
