-- This is a test control file

test_name veen-called-other-viol
test_description A validity violation involving a construct enclosing an Entity rule violation

copy_sub	Ace $TEST Ace
copy_raw	test.e $CLUSTER test.e
compile_melted
compile_result	validity_error TEST VEEN

copy_raw	test_rev1.e $CLUSTER test.e
resume_compile
compile_result	validity_error TEST VEEN

copy_raw	test_rev2.e $CLUSTER test.e
resume_compile
compile_result	validity_error TEST VEEN

test_end
