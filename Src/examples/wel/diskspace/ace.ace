system
	diskspace_demo

root 
	DISKSPACE_DEMO (root_cluster): "make"

default
	assertion (all)
	console_application (yes)

cluster 
	root_cluster:	"$ISE_LIBRARY\examples\wel\diskspace"

		-- EiffelBase
	all base: 		"$ISE_LIBRARY\library\base"
		exclude
			"table_eiffel3"
			"desc"
		end

		-- WEL
	all wel: 		"$ISE_LIBRARY\library\wel"
		exclude
			"spec"
			"clib"
		end

external
	include_path:	"$(ISE_LIBRARY)\library\wel\spec\windows\include"
	object: 		"$(ISE_LIBRARY)\library\wel\spec\$(ISE_C_COMPILER)\lib\wel.lib"

end -- diskspace_demo
