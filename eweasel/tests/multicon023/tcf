-- This is a test control file

test_name multi-constraint-add-gen-constraint
test_description After initial compilation, a constraint is added to an unconstrained formal generic parameter of a generic class

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
copy_raw 	test1_rev1.e $CLUSTER test1.e

compile_melted
compile_result 	validity_error TEST1 VTGC(2)

copy_raw 	test1_rev2.e $CLUSTER test1.e

resume_compile
compile_result 	validity_error TEST1 VTGC(3)


test_end
