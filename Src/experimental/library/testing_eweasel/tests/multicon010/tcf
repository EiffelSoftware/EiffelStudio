-- This is a test control file

test_name multi-constraint-inline-agents
test_description Using inline agents on features of multi constraint formal generic type parameter.

---------------------------------------------
define TYPE "INTEGER_32_REF"

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e

compile_melted
compile_result 	ok
execute_work NONE melted_output1
compare melted_output1 output_integer_32_ref

compile_frozen
compile_result 	ok
c_compile_work
c_compile_result ok

execute_work NONE frozen_output1
compare frozen_output1 output_integer_32_ref

compile_final
compile_result 	ok
c_compile_final
c_compile_result ok

execute_final NONE final_output1
compare final_output1 output_integer_32_ref
---------------------------------------------
