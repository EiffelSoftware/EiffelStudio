-- This is a test control file

test_name 	manifest-array-of-bits
test_description A system which concatenates and prints the BIT constants of a manifest array many times

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work NONE 	exec_output1 10000
execute_result	ok
compare		exec_output1 output1

test_end
