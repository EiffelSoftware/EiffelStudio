<?xml version="1.0" encoding="ISO-8859-1"?><system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="cecil" uuid="DAACB8CE-DA51-4D25-9277-1194904B3A08">
	<target name="classic">
		<root cluster="root_cluster" class="ROOT_CLASS" feature="make"/>
