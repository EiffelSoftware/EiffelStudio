		<library name="persistence_utils" location="../persistence_utils/persistence_utils.ecf"/> -->
		<library name="abel_core" location="$ISE_LIBRARY\unstable\library\persistency\abel\core\core.ecf"/>
		<library name="curl" location="$ISE_LIBRARY/library/cURL/cURL-safe.ecf"/>
		<cluster name="abel_backend_couchdb" location=".\" recursive="true">
			<file_rule>
				<exclude>/EIFGENs$</exclude>
				<exclude>/.svn$</exclude>
				<exclude>/CVS$</exclude>
			</file_rule>
		</cluster>
