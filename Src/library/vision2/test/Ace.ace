system

dynamic_list_test	

root

	EV_DYNAMIC_LIST_TEST: make_and_launch

default

	assertion (all)

	--debug("EVENT_TRACE")
	--debug("EV_GTK_DEBUG")
cluster

dev:						"$WS"

event(dev):					"$/event"

gel(dev):					"$/gel"

all vision2(dev):				"$/vision2"
	exclude
		"mswin";
		"new_figures";
		"EIFGEN";
		"release";
		"obsolete";
	end

all base(dev):					"$/base"
	exclude
		"table_eiffel3"
	end

external

	include_path:
		"$WS/vision2/implementation/gtk/Clib", "`gtk-config --cflags`",
		"$WS/vision2/spec/include";
	object:
		"$(WS)/vision2/implementation/gtk/Clib/gtk_eiffel.o -lpng -lz -lXtst",
		"$(WS)/vision2/Clib/load_pixmap.o",
		"`gtk-config --libs`"

end
