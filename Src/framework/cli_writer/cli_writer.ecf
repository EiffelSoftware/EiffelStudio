<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-10-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-10-0 http://www.eiffel.com/developers/xml/configuration-1-10-0.xsd" name="cli_writer" uuid="BE7B7959-6C6E-4C17-8B5C-E3D5E27D876D" library_target="default">
	<target name="default">
		<root all_classes="true"/>
		<file_rule>
			<exclude>/\.svn$</exclude>
		</file_rule>
		<option is_attached_by_default="false" syntax="transitional">
			<assertions precondition="true"/>
		</option>
		<external_include location="$(EIFFEL_SRC)\framework\cli_writer\Clib">
			<condition>
				<platform value="windows"/>
			</condition>
		</external_include>
		<external_cflag value="-D_WIN32_DCOM">
			<condition>
				<platform value="windows"/>
			</condition>
		</external_cflag>
		<external_object location="$(EIFFEL_SRC)\framework\cli_writer\spec\$(ISE_C_COMPILER)\$(ISE_PLATFORM)\lib\cli_writer.lib">
			<condition>
				<platform value="windows"/>
			</condition>
		</external_object>
		<library name="base" location="$ISE_LIBRARY\library\base\base.ecf"/>
		<library name="com_light" location="$EIFFEL_SRC\framework\com_light\com_light.ecf"/>
		<library name="dotnet_loader" location="$EIFFEL_SRC\framework\dotnet_loader\dotnet_loader.ecf"/>
		<library name="environment" location="$EIFFEL_SRC\framework\environment\environment.ecf"/>
		<library name="file_utilities" location="$EIFFEL_SRC\framework\file_utilities\file_utilities.ecf"/>
		<library name="wel" location="$ISE_LIBRARY\library\wel\wel-safe.ecf">
			<condition>
				<platform value="windows"/>
			</condition>
		</library>
		<cluster name="cli_writer" location=".\" recursive="true">
			<file_rule>
				<exclude>/sample$</exclude>
				<exclude>/Clib$</exclude>
				<exclude>/spec$</exclude>
			</file_rule>
			<file_rule>
				<exclude>/pe_file$</exclude>
				<exclude>/emitter$</exclude>
				<exclude>/implementation/windows$</exclude>
				<exclude>/fusion$</exclude>
				<condition>
					<platform excluded_value="windows"/>
				</condition>
			</file_rule>
			<file_rule>
				<exclude>/implementation/unix$</exclude>
				<condition>
					<platform value="windows"/>
				</condition>
			</file_rule>
		</cluster>
	</target>
	<target name="sample" extends="default">
		<root class="ROOT_CLASS" feature="make"/>
		<cluster name="sample" location="sample\"/>
	</target>
</system>
