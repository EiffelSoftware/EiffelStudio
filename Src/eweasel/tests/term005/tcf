-- This is a test control file

test_name 	like-current-generic-constraint
test_description A constrained generic class which is syntactically illegal because the constraint is `like Current'

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
copy_raw 	test1.e $CLUSTER test1.e
compile_melted
compile_result 	syntax_error TEST1 8

test_end

