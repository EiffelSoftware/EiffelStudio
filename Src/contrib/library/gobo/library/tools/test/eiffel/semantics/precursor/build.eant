<?xml version="1.0"?>

<project name="precursor" default="help">

	<description>
		system: "ECMA precursor semantics Tester"
		copyright: "Copyright (c) 2008-2017, Eric Bezault and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/library/common/config/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>

<!-- Implementation -->

	<target name="init_system" export="NONE">
		<set name="system" value="xprecursor"/>
		<set name="category" value="precursor"/>
		<set name="system_dir" value="${GOBO}/library/tools/test/eiffel/semantics/${category}"/>
	</target>

</project>
