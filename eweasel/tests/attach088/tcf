define	C	"-- "
copy_sub	test.e $CLUSTER test.e

resume_compile
compile_result validity_error TEST VEVI VEVI VEVI VEVI VEVI VEVI VEVI VJAR VJAR VJAR VJAR VJAR VJAR VJAR VJAR VJAR VJAR

test_end
