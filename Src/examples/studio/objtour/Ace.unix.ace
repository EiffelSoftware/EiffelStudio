system
	simple
	
root
	starter  (root_cluster): "make"

default
	precompiled ("$ISE_EIFFEL/precomp/spec/$ISE_PLATFORM/base")

cluster
	
	root_cluster:	"$ISE_EIFFEL/examples/studio/objtour";
				-- If you intend to modify any part of the example,
				-- copy all the files of $ISE_EIFFEL/examples/studio/objtour
				-- into one of your directories, and replace
				-- $ISE_EIFFEL/examples/studio/objtour by the name of
				-- that directory on the above line.

		-- EiffelBase:
	all base:		"$ISE_EIFFEL/library/base"
		exclude
			"desc";"table_eiffel3"
		end



end
