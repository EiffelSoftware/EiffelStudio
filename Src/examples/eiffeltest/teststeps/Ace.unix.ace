system
	seqtest

root
	TEST_ROOT(root_cluster): "make"

default
	assertion (all)
	precompiled ("$ISE_EIFFEL/precomp/spec/$ISE_PLATFORM/base")

cluster
	root_cluster:	"."

	all time:	"$ISE_EIFFEL/library/time"
		exclude
			"german"; "french"
		end

	all eiffeltest: "$ISE_EIFFEL/library/eiffeltest"

external
	object:
		"\$(ISE_EIFFEL)/library/time/spec/\$(ISE_PLATFORM)/lib/libdatetime.a"

end
