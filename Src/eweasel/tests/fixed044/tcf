-- This is a test control file

test_name 	 invalid-actual-generic6
test_description An actual generic parameter `TEST1 [like Current]' does not conform to a formal generic parameter constrained by `TEST2 [like Current]'

copy_sub 	 Ace $TEST Ace
copy_raw 	 test.e  $CLUSTER test.e
copy_raw 	 test1.e  $CLUSTER test1.e
copy_raw 	 test2.e  $CLUSTER test2.e
compile_melted
compile_result 	 validity_error TEST2 VTCG; TEST1 VCFG(3)

test_end
