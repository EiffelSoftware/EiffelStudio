system
	calculator 

root

	calculator (ROOT_CLUSTER): "make"

default
	assertion (all);
	precompiled ("$ISE_EIFFEL/precomp/spec/$ISE_PLATFORM/base")

cluster

	ROOT_CLUSTER:		"$ISE_EIFFEL/examples/base/calculator";
				-- If you intend to modify any part of the example,
				-- copy all the files of $ISE_EIFFEL/examples/base/calculator
				-- into one of your directories, and replace
				-- $ISE_EIFFEL/examples/base/calculator by the name of
				-- that directory on the above line.

		-- EiffelBase:
	all base:		"$ISE_EIFFEL/library/base"
		exclude
			"desc";"table_eiffel3"
		end

end
