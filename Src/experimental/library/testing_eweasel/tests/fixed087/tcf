-- This is a test control file

test_name 	 none-conform-bit-expanded
test_description Assignment of Void to entity of an an expanded type or a Bit_type

copy_sub 	 Ace $TEST Ace
define		 INSTRUCTION "y := Void"
copy_sub 	 test.e  $CLUSTER test.e
copy_raw 	 test1.e  $CLUSTER test1.e
compile_melted
compile_result 	 validity_error TEST VJAR

define		 INSTRUCTION "x := Void"
copy_sub 	 test.e  $CLUSTER test.e
resume_compile
compile_result 	 validity_error TEST VJAR

test_end
