#!/bin/csh -f
# 
# EiffelStore C-libraries compiling script
# (c) SOL 1993, 1994
#

echo EiffelStore C-libraries are being installed ...

set current_dir = `pwd`

cd $EIFFEL4/library/matisse

find . -name "Makefile.SH" -exec $EIFFEL4/library/matisse/install/build_a_Clib {} \;

cd $current_dir

echo All done.

exit
