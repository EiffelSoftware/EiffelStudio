 CONTROL "Text sample.", IDC_EDIT, "EDIT", ES_MULTILINE | WS_BORDER | WS_VSCROLL | WS_HSCROLL | WS_GROUP | WS_TABSTOP, 111, 5, 139, 114
 LTEXT "&Size", -1, 258, 3, 19, 8
 CONTROL "30", IDC_EDIT_SIZE, "EDIT", WS_BORDER | WS_GROUP | WS_TABSTOP, 258, 12, 18, 12
 PUSHBUTTON "&Close", IDC_CLOSE_BUTTON, 256, 107, 50, 14, WS_GROUP | WS_TABSTOP
}
