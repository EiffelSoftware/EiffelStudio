system
	"EiffelSoftware.CodedomSerializer"

root 
	none

default
	assertion (no)
	msil_generation (yes)
	msil_clr_version ("v1.1.4322")
	msil_generation_type ("dll")
	msil_key_file_name ("$EIFFEL_SRC\dotnet\helpers\isekey.snk")
	msil_use_optimized_precompile (yes)
	dotnet_naming_convention (yes)
	version ("2.0.0.0")
	use_cluster_name_as_namespace (yes)
	use_all_cluster_name_as_namespace (yes)
	cls_compliant (yes)
	il_verifiable (yes)
	console_application (no)
	line_generation (no)
	precompiled ("$EIFFEL_SRC\dotnet\codedom_provider\build_studio_debug\EiffelSoftware.EiffelVision2")

cluster 
		-- The application
	all serializer: "$EIFFEL_SRC\dotnet\codedom_provider\test\serializer"
		exclude
			"CVS"; "gui"
		end


	codedom_provider:		"$EIFFEL_SRC\dotnet\codedom_provider\common"
		exclude
			"code_shared_metadata_access.e"; "CVS";
		end


	all "helpers":		"$EIFFEL_SRC\dotnet\helpers"
		exclude
			"CVS"; 
		end

end
