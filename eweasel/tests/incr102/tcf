-- This is a test control file

test_name 	 bigger-bit-func-wrong-value
test_description After initial compilation, a constant integer attribute referenced in a BIT type which is the return type of a function is given a different (larger) value.  After recompilation, the function should return a larger BIT type during system execution.

copy_sub 	 Ace $TEST Ace
define		 BIT_LENGTH "10"
copy_sub 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

execute_work 	 NONE exec_output1
execute_result   ok
compare 	 exec_output1 output1

define		 BIT_LENGTH "11"
copy_sub 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

execute_work 	 NONE exec_output2
execute_result   ok
compare 	 exec_output2 output2

test_end
