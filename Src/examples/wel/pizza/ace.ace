system
	pizza_demo

root 
	PIZZA_DEMO (root_cluster): "make"

default
	assertion (all)

cluster 
	root_cluster:	"$EIFFEL4\examples\wel\pizza"

		-- WEL
	all wel: 		"$EIFFEL4\library\wel"

		-- EiffelBase
	all base: 		"$EIFFEL4\library\base"
		exclude
			"table_eiffel3"
			"desc"
		end

external
	include_path:	"$(EIFFEL4)\library\wel\spec\windows\include"
	object: 		"$(EIFFEL4)\library\wel\spec\$(COMPILER)\lib\wel.lib"

end -- pizza_demo
