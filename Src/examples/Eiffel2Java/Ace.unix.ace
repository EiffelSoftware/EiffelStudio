system
	sample

root
	ROOT_CLASS: "make"

default
	assertion (all)
	address_expression (yes)

cluster

	root_cluster: "$ISE_EIFFEL/examples/Eiffel2Java";

		-- EiffelBase
	all base:						"$ISE_EIFFEL/library/base"
		exclude
			"desc";"table_eiffel3"
		end

	all java:		"$ISE_EIFFEL/library/Eiffel2Java"

external
	include_path:
		"$(JDK_HOME)/include"

	object:
		"-L$(JDK_HOME)/lib -ljava"
		

end
