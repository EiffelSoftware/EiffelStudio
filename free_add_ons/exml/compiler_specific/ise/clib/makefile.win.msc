# exml.lib - Makefile for Microsoft C

CC = cl
CFLAGS = -c -Ox -I$(EXML)\compiler_specific\ise\clib -I$(EXPAT)xmlparse -I$(EIFFEL4)\bench\spec\windows\include 
.c.obj:
	$(CC) $(CFLAGS) $<

OBJ = exml_parser.obj

exml.lib: $(OBJ)
	if exist $@ del $@
	lib /OUT:$@ $(OBJ)
	if not exist $(EXML)\compiler_specific\ise\clib\msc mkdir $(EXML)\compiler_specific\ise\clib\msc
	copy $@ $(EXML)\compiler_specific\ise\clib\msc
	del $@
	del *.obj
