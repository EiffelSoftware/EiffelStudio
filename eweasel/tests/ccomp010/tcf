-- This is a test control file

test_name 	frozen-bit-operations
test_description The C code generated for some bit operations will not compile.  I believe these operations are `not', infix "^" and infix "#".

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
compile_melted
compile_result 	ok

execute_work	NONE exec_output1
execute_result  ok
compare		exec_output1 output1

compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work	NONE exec_output2
execute_result  ok
compare		exec_output2 output1

test_end
