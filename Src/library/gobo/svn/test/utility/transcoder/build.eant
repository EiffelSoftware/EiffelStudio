<?xml version="1.0"?>

<project name="transcoder" default="help">

	<description>
		system: "Gobo Eiffel Transcoder Library Tester"
		copyright: "Copyright (c) 2005, Colin Adams and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/misc/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xtranscoder"/>
		<set name="library" value="utility"/>
		<set name="system_dir" value="${GOBO}/test/${library}/transcoder"/>
	</target>

</project>
