system
	"emitter"

root
	EMITTER: make

default
	namespace ("ISE.Cache")
	assertion (no)
	msil_generation (yes)
	arguments (" ")
	disabled_debug (yes)
	debug (no)
	line_generation (yes)
	il_verifiable (yes)
	cls_compliant (yes)
	dotnet_naming_convention (no)
	msil_generation_type ("exe")
	check_vape (yes)
	console_application (yes)


-- EiffelBase

cluster
	all root_cluster:		"$EIFFEL_SRC\dotnet\consumer\emitter"
		exclude
			"EIFGEN"; "CVS"; 
		end

	all base:		"$EIFFEL_SRC\library\base"
		exclude
			"table_eiffel3"; "desc"; "classic"; "CVS"; 
		end

	all base_net:		"$EIFFEL_SRC\library.net\base"
		exclude
			"CVS"; "support.net"; "system_object.e"
		end

	helpers:		"$EIFFEL_SRC\dotnet\helpers"
		exclude
			"CVS"; 
		end

	components:		"$EIFFEL_SRC\dotnet\consumer\components"
		exclude
			"CVS"; 
		end

	xml_serializer:		"$EIFFEL_SRC\dotnet\consumer\serialization"
		exclude
			"CVS"; 
		end

	all support:		"$EIFFEL_SRC\dotnet\consumer\support"
		exclude
			"CVS"; 
		end

	cache:		"$EIFFEL_SRC\dotnet\consumer\cache"
		exclude
			"CVS"; 
		end

	all wel:		"$EIFFEL_SRC\library\wel"
		exclude
			"CVS"; 
		end

assembly
	mscorlib:   "mscorlib",   "1.0.3300.0", "neutral", "b77a5c561934e089"
	system_:    "System",     "1.0.3300.0", "neutral", "b77a5c561934e089"
	system_xml: "System.Xml", "1.0.3300.0", "neutral", "b77a5c561934e089"

end
