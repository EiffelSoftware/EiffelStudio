test_name property-incremental-add-remove
test_description Checks the compile handles incrmental adding an removing of property declarations

copy_sub Ace $TEST Ace
copy_raw test.e $CLUSTER test.e
copy_raw base.e $CLUSTER base.e

compile_melted
compile_result	ok

execute_work 	NONE 	exec_output
execute_result 	ok
compare		exec_output output

copy_raw base_2.e $CLUSTER base.e

compile_melted
compile_result	ok

execute_work 	NONE 	exec_output_2
execute_result 	ok
compare		exec_output_2 output_2

copy_raw base.e $CLUSTER base.e

compile_melted
compile_result	ok

execute_work 	NONE 	exec_output_3
execute_result 	ok
compare		exec_output_3 output

test_end
