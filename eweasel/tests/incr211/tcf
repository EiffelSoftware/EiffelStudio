-- This is a test control file

test_name 	 uninherit-any-vupr2-sneak
test_description After initial compilation, a VUPR(2) error is introduced by removing an inheritance clause for ANY, where a routine is calling `Precursor {ANY}'.  The compiler does not detect the newly introduced VUPR(2) error.

define PARENT "ANY redefine default_rescue end"
copy_sub 	Ace $TEST Ace
copy_sub 	test.e $CLUSTER test.e
copy_raw 	test2.e $CLUSTER test2.e
compile_melted
compile_result 	ok

define PARENT ""
copy_sub 	test.e $CLUSTER test.e
compile_melted
compile_result 	validity_error TEST VUPR(2)

test_end
