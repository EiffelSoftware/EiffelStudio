-- This is a test control file

test_name 	 wrong-dynamic-type
test_description Querying the dynamic type of a non-generic class returns -1 instead of a valid dynamic type

copy_sub 	 Ace $TEST Ace
copy_raw 	 test.e  $CLUSTER test.e
compile_melted
compile_result 	 ok

execute_work	 NONE exec_output1
execute_result	 ok
compare		 exec_output1 output

test_end
