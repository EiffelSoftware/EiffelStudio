test_name 	bogus-bit-vdrd
test_description A redefinition of BIT 2 into BIT 1 should be accepted instead of reporting a VDRD error.

copy_sub 	Ace $TEST Ace

copy_raw 	test.e $CLUSTER test.e
copy_raw 	x.e $CLUSTER x.e
copy_raw 	y.e $CLUSTER y.e

compile_melted
compile_result 	ok

execute_work 	NONE 	exec_output1
execute_result 	ok
compare		exec_output1 output

compile_frozen
compile_result 	ok

c_compile_work
c_compile_result ok

execute_work 	NONE 	exec_output2
execute_result 	ok
compare		exec_output2 output

test_end
