AR = ar rc
CC = cc
CTAGS = ctags
JCFLAGS = $(CFLAGS) 
LINT = lint
MAKE = make
MKDEP = /home/tomh/Eiffel3/C/mkdep $(DPFLAGS) --
MV = /bin/mv
RANLIB = /usr/bin/ranlib
RM = DELETE/nolog
SED = /bin/sed

