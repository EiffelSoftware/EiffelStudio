indexing
	description: "Strings used in the Eiffel syntax"
	author: "Xavier Rousselot"
	date: "$Date$"
	revision: "$Revision$"

class
	SYNTAX_STRINGS

feature
	Prefix_str: STRING is "prefix %""
	Infix_str: STRING is "infix %""
	Quote_str: STRING is "%""
	Frozen_str: STRING is "frozen "

end -- class SYNTAX_STRINGS
