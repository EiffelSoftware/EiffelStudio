system
	SYSTEM_NAME
	-- Replace SYSTEM_NAME by the name of the executable file
	-- to be generated for your system.

root

	ROOT_CLASS (ROOT_CLUSTER): "creation_procedure"
	-- Replace ROOT_CLASS, ROOT_CLUSTER and creation_procedure
	-- by the names of the root class, root class cluster and
	-- root creation procedure for your system.
	-- The `(ROOT_CLUSTER)' part may be omitted if there is
	-- no other class of name ROOT_CLASS in the universe.

default

	assertion (require);
	precompiled ("$EIFFEL5/precomp/spec/$PLATFORM/mvision")

cluster

	ROOT_CLUSTER: 	"PATH";
		-- Replace ROOT_CLUSTER and PATH by the names of the
		-- root class cluster and corresponding path for your system.
		-- Add any other clusters that your system will need.

-- EiffelBase

		access:				"$EIFFEL5/library/base/structures/access";
		cursors:			"$EIFFEL5/library/base/structures/cursors";
		cursor_tree:		"$EIFFEL5/library/base/structures/cursor_tree";
		dispenser:			"$EIFFEL5/library/base/structures/dispenser";
		iteration:			"$EIFFEL5/library/base/structures/iteration";
		kernel:				"$EIFFEL5/library/base/kernel";
		list:				"$EIFFEL5/library/base/structures/list";
		obsolete:			"$EIFFEL5/library/base/structures/obsolete";
		set:				"$EIFFEL5/library/base/structures/set";
		sort:				"$EIFFEL5/library/base/structures/sort";
		storage:			"$EIFFEL5/library/base/structures/storage";
		support:			"$EIFFEL5/library/base/support";
		table:				"$EIFFEL5/library/base/structures/table";
		traversing:			"$EIFFEL5/library/base/structures/traversing";
		tree:				"$EIFFEL5/library/base/structures/tree";

-- EiffelVision

		graph_resources:	"$EIFFEL5/library/vision/implement/motif/Resources";
		graph_widgets:		"$EIFFEL5/library/vision/implement/motif/widgets";
		graph_toolkit:		"$EIFFEL5/library/vision/implement/toolkit";
		graph_X:			"$EIFFEL5/library/vision/implement/X";
		graph_commands:		"$EIFFEL5/library/vision/oui/commands";
		graph_kernel:		"$EIFFEL5/library/vision/oui/kernel";
		graph_oui_widgets:	"$EIFFEL5/library/vision/oui/widgets";
		graph_tools:		"$EIFFEL5/library/vision/tools";
		graph_figures:		"$EIFFEL5/library/vision/figures";

external

	object:

		"$(EIFFEL5)/library/vision/spec/$(PLATFORM)/lib/motif_Clib.a";
		"$(EIFFEL5)/library/vision/spec/$(PLATFORM)/lib/Xt.a";
		"-lXm -lXt -lX11"
			
end
