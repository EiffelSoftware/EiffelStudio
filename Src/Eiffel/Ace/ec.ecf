	<target name="batch" extends="common">
		<root class="ES" feature="make"/>
		<variable name="batch_preferences" value="true"/>
		<variable name="batch_framework" value="true"/>
		<library name="preferences_reg" location="$ISE_LIBRARY\library\preferences\preferences_reg.ecf" readonly="false">
			<condition>
				<platform value="windows"/>
			</condition>
		</library>
		<library name="preferences_xml" location="$ISE_LIBRARY\library\preferences\preferences_xml.ecf" readonly="false">
			<condition>
				<platform excluded_value="windows"/>
			</condition>
		</library>
		<cluster name="beurk" location="$EIFFEL_SRC\Eiffel\beurk\"/>
		<cluster name="compiler" location="$EIFFEL_SRC\Eiffel\eiffel\" recursive="true">
			<file_rule>
				<exclude>/Core$</exclude>
				<exclude>/kernel$</exclude>
				<exclude>/parser_override$</exclude>
			</file_rule>
			<file_rule>
				<exclude>/com_il_generation$</exclude>
				<exclude>/windows$</exclude>
				<condition>
					<platform excluded_value="windows"/>
				</condition>
			</file_rule>
			<file_rule>
				<exclude>/unix$</exclude>
				<exclude>/Clib$</exclude>
				<condition>
					<platform value="windows"/>
				</condition>
			</file_rule>
		</cluster>
		<cluster name="compiler_api" location="$EIFFEL_SRC\Eiffel\API\" recursive="true">
			<file_rule>
				<exclude>/structured_text$</exclude>
				<exclude>/queries$</exclude>
				<exclude>/query_language$</exclude>
				<exclude>/profiler$</exclude>
			</file_rule>
			<file_rule>
				<exclude>/eb_preference_manager.e$</exclude>
				<condition>
					<platform value="windows"/>
				</condition>
			</file_rule>
		</cluster>
		<cluster name="compiler_api_profiler" location="$EIFFEL_SRC\Eiffel\API\profiler\" recursive="true"/>
		<cluster name="compiler_api_ql" location="$EIFFEL_SRC\Eiffel\API\query_language\" recursive="true"/>
		<cluster name="compiler_api_queries" location="$EIFFEL_SRC\Eiffel\API\queries\" recursive="true"/>
		<cluster name="documentation" location="$EIFFEL_SRC\Eiffel\case\documentation\" recursive="true"/>
		<cluster name="flat_short_b" location="$EIFFEL_SRC\Eiffel\flat_short\bench\"/>
		<cluster name="interface" location="$EIFFEL_SRC\Eiffel\interface\">
			<cluster name="interface_shared" location="$|shared\" recursive="true"/>
			<cluster name="tty" location="$|tty\" recursive="true"/>
		</cluster>
		<cluster name="switch" location="$EIFFEL_SRC\Eiffel\switch\">
			<cluster name="no_communications" location="$|no_communication\">
				<cluster name="debug_class" location="$|debug\"/>
				<cluster name="status" location="$|status\"/>
			</cluster>
			<cluster name="interface_common" location="$|interface\"/>
			<cluster name="interface_shell" location="$|no_communication\shell\"/>
			<cluster name="interface_switch" location="$|interface\batch_only\"/>
		</cluster>
		<override name="parser_override" location="$EIFFEL_SRC\Eiffel\eiffel\AST\parser_override\"/>
	</target>
	<target name="ecdpc" extends="batch">
		<setting name="executable_name" value="ecdpc"/>
	</target>
