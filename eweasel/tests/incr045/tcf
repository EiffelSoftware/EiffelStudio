-- This is a test control file

test_name 	chg-integer-const-value
test_description After initial compilation, the value of a constant integer attribute is changed and the system is recompiled

copy_sub 	Ace $TEST Ace
copy_raw 	test.e $CLUSTER test.e
compile_melted
compile_result 	ok

execute_work NONE exec_output1
execute_result  ok
compare exec_output1 output1

copy_raw 	test_rev1.e $CLUSTER test.e
compile_melted
compile_result 	ok

execute_work NONE exec_output2
execute_result ok
compare exec_output2 output2

test_end
