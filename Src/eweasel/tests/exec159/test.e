class TEST
inherit
	EXCEPTIONS

create
	make
feature
	make is
		local
			tried: BOOLEAN
			s: STRING
		do
			if not tried then
				
			else
				s := exception_trace
				print ("Retrieved exception trace%N")
			end
		rescue
			tried := True
			retry
		end

	 is
		do
			io.put_string (Void)
		end

end
