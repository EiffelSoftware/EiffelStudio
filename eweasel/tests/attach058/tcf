test_name 	 void-safety-level
test_description Different void safety levels produce different results,

copy_raw	a.e $CLUSTER a.e
copy_raw	test.e $CLUSTER test.e

define VOID_SAFE_A "true"
define VOID_SAFE_TEST "true"
copy_sub	Ace $TEST Ace
compile_melted
compile_result	validity_error TEST VUAR(2) VEVI VUTA(2) VUTA(2) VJAR; A VUTA(2) VJAR
abort_compile

define VOID_SAFE_A "false"
define VOID_SAFE_TEST "true"
copy_sub	Ace $TEST Ace
compile_melted
compile_result	validity_error TEST VUAR(2) VEVI VUTA(2) VUTA(2) VJAR
abort_compile

define VOID_SAFE_A "true"
define VOID_SAFE_TEST "false"
copy_sub	Ace $TEST Ace
compile_melted
compile_result	validity_error A VUTA(2) VJAR
abort_compile

define VOID_SAFE_A "false"
define VOID_SAFE_TEST "false"
copy_sub	Ace $TEST Ace
compile_melted
compile_result	ok
cleanup_compile

test_end
