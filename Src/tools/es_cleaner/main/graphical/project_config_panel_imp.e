			l_ev_cell_1.set_minimum_width (13)
			grp_project.resize (2, 2)
			grp_project.set_row_spacing (4)
			grp_project.set_column_spacing (4)
				-- Insert and position all children of `grp_project'.
			grp_project.put_at_position (l_ev_label_1, 1, 1, 1, 1)
			grp_project.put_at_position (l_ev_horizontal_box_2, 2, 1, 1, 1)
			grp_project.put_at_position (l_ev_cell_2, 1, 2, 1, 1)
			grp_project.put_at_position (l_ev_horizontal_box_3, 2, 2, 1, 1)
			l_ev_label_1.set_text ("ECF Path:")
