<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-0-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-0-0 http://www.eiffel.com/developers/xml/configuration-1-0-0.xsd" name="preferences" uuid="B453E790-C89E-45DC-BA9C-E00E4C4BE87A" library_target="preferences">
  <target name="preferences">
    <description>
      Preferences library.
      Copyright (c) 1984-2006, Eiffel Software and others.
      Eiffel Forum License v2 (see http://www.eiffel.com/licensing/forum.txt).
    </description>
    <root all_classes="true"/>
    <file_rule>
      <exclude>/\.svn</exclude>
    </file_rule>
    <option namespace="EiffelSoftware.Library"/>
    <setting name="dotnet_naming_convention" value="true"/>
    <library name="base" location="$ISE_LIBRARY\library\base\base.ecf"/>
    <library name="gobo" location="$ISE_LIBRARY\library\gobo\gobo.ecf"/>
    <library name="vision2" location="$ISE_LIBRARY\library\vision2\vision2.ecf">
      <condition>
        <custom name="batch_preferences" excluded_value="true"/>
      </condition>
    </library>
    <library name="wel" location="$ISE_LIBRARY\library\wel\wel.ecf">
      <condition>
        <custom name="batch_preferences" excluded_value="true"/>
      </condition>
    </library>
    <cluster name="interface" location="interface\" recursive="true">
      <option namespace="Preferences.Interface"/>
      <condition>
        <custom name="batch_preferences" excluded_value="true"/>
      </condition>
    </cluster>
    <cluster name="interface_batch" location="interface\" recursive="true">
      <option namespace="Preferences.Interface"/>
      <condition>
        <custom name="batch_preferences" value="true"/>
      </condition>
      <file_rule>
        <exclude>/graphical$</exclude>
        <exclude>/build_project$</exclude>
      </file_rule>
    </cluster>
    <cluster name="preference" location="preference\" recursive="true">
      <option namespace="Preferences"/>
      <condition>
        <custom name="batch_preferences" excluded_value="true"/>
      </condition>
    </cluster>
    <cluster name="preference_batch" location="preference\" recursive="true">
      <option namespace="Preferences"/>
      <condition>
        <custom name="batch_preferences" value="true"/>
      </condition>
      <file_rule>
        <exclude>/graphical$</exclude>
      </file_rule>
    </cluster>
    <cluster name="preferences" location=".\">
      <option namespace="Preferences"/>
    </cluster>
    <cluster name="storage" location="storage\" recursive="true">
      <option namespace="Storage"/>
      <file_rule>
        <exclude>/registry_implementation$</exclude>
      </file_rule>
    </cluster>
  </target>
</system>
